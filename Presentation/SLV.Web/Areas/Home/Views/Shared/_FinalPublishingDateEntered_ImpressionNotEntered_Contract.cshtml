
<style type="text/css">
    .table-color a {
        color: #ee5941;
    }
</style>
<div class="portlet box green">
    <div class="portlet-title">
        <div class="caption">
            @T("Dashboard.ImpressionNotEntered")
        </div>
        <div class="tools">
            <a href="javascript:;" class="collapse"> </a>
        </div>
    </div>
    <div class="portlet-body">
        <div class="table-responsive">
            <div ng-hide="!ImpressionNotEntered_List_AuthorContract.length">
                <table class="table table-bordered table-striped">
                    <tr>
                        <th> Contract/License Code </th>
                        <th> Product Code </th>
                        <th> ISBN </th>

                        <th> Publishing Company Name </th>
                        <th> Requested By </th>
                        <th> Requested Date </th>


                        <th> Working Product</th>
                        <th> Author </th>
                        <th> Title </th>
                    </tr>

                    <tr ng-repeat="Product in ImpressionNotEntered_List_AuthorContract">
                        <td class="table-color"><a href="~/Product/ProductLicense/ImpressionEntry?ProductId={{Product.Id}}&ContractId={{Product.ContractId}}"> <span ng-bind="(Product.Contract_Code)"></span>   </a></td>
                        <td><span ng-bind="(Product.ProductCode)"></span></td>
                        <td> <span ng-bind="(Product.ISBN)"></span></td>

                        <td>--</td>
                        <td><span ng-bind="(Product.ExecutiveName)"></span> </td>
                        <td> <span ng-bind="(Product.EntryDate)"></span></td>

                        <td> <span ng-bind="(Product.WorkingProduct)"></span><span style="font-size:11px;">  <br /><span ng-bind="(Product.WorkingSubProduct)"></span></span></td>
                        <td><span ng-bind="(Product.AuthorName)"></span> </td>
                        <td> <span ng-bind="(Product.FinalProductName)"></span> </td>
                    </tr>
                    <tr ng-repeat="Product in ImpressionNotEntered_List">
                        <td class="table-color"><a href="~/Product/ProductLicense/ImpressionEntry?ProductId={{Product.Id}}&LicenseId={{Product.ProductLicenseId}}"> <span ng-bind="(Product.ProductLicensecode)"></span>  </a></td>
                        <td> <span ng-bind="(Product.ProductCode)"></span>  </td>
                        <td><span ng-bind="(Product.ISBN)"></span>   </td>

                        <td><span ng-bind="(Product.PublishingCompany)"></span>  </td>
                        <th>--</th>
                        <th>--</th>

                        <td> <span ng-bind="(Product.WorkingProduct)"></span> <span style="font-size:11px;">  <br />  <span ng-bind="(Product.WorkingSubProduct)"></span> </span></td>
                        <td><span ng-bind="(Product.AuthorName)"></span>  </td>
                        <td><span ng-bind="(Product.FinalProductName)"></span>  </td>
                    </tr>
                </table>
                <a href="~/Product/ProductLicense/ImpressionSearch" class="moreDash">View More</a>
            </div>
            <div ng-hide="ImpressionNotEntered_List_AuthorContract.length">No result found</div>
        </div>
    </div>
</div>



