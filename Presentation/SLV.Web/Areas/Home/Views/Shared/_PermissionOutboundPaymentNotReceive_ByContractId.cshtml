

<style type="text/css">
    .table-color a {
        color: #ee5941;
    }
</style>
<div class="portlet box green">
    <div class="portlet-title">
        <div class="caption">
            @T("Dashboard.OutboundPaymentNotReceived")
        </div>
        <div class="tools">
            <a href="javascript:;" class="collapse"> </a>
        </div>
    </div>
    <div class="portlet-body">
        <div class="table-responsive">
            <div ng-hide="!PermissionOutboundPaymentNotReceive_ByContractId_List.length && !PermissionOutboundPaymentNotReceive_ByLicenseId_List.length">
                <table class="table table-bordered table-striped">
                    <tr>
                        <th> Contract/License Code </th>
                        <th> Product Code </th>
                        <th> Licensee Name </th>
                        <th> Requested By </th>
                        <th> Requested Date </th>
                        <th> Permissions Outbound Code </th>
                        <th> Working Product </th>
                        <th>Type </th>
                    </tr>

                    <!--Contract wise-->
                    <tr ng-repeat="RightSalePNR in PermissionOutboundPaymentNotReceive_ByContractId_List">
                        @*<td class="table-color"><a href="~/PermissionsOutbound/PermissionsOutbound/PermissionsOutboundPaymentTagging?AuthorContractId={{RightSalePNR.Id}}">  <span ng-bind="(RightSalePNR.Contract_Code)"></span> </a></td>*@


                        <td class="table-color"><a href="~/PermissionsOutbound/PermissionsOutbound/PermissionsOutboundPaymentTagging?AuthorContractId={{RightSalePNR.Id}}&OutboundId={{RightSalePNR.PermissionsOutboundId}}&ProuductId={{RightSalePNR.ProductId}}">  <span ng-bind="(RightSalePNR.Contract_Code)"></span> </a></td>

                        <td class="table-color"><a href="~/Product/ProductMaster/ProductDetailsView?Id={{RightSalePNR.ProductId}}"> <span ng-bind="(RightSalePNR.ProductCode)"></span></a> </td>

                        <td><span ng-bind="(RightSalePNR.OrganizationName)"></span> </td>
                        <td> <span ng-bind="(RightSalePNR.ExecutiveName)"></span> </td>
                        <td> <span ng-bind="(RightSalePNR.EntryDate)"></span> </td>


                        <td><span ng-bind="(RightSalePNR.PermissionsOutboundCode)"></span> </td>
                        <td><span ng-bind="(RightSalePNR.WorkingProduct)"></span>  <span style="font-size:11px;">  <br /> <span ng-bind="(RightSalePNR.WorkingSubProduct)"></span> </span></td>
                        <td>Contract</td>
                    </tr>@*PermissionsOutbound/PermissionsOutbound/PermissionsOutboundPaymentTagging?AuthorContractId*@

                    <!--License wise-->
                    <tr ng-repeat="RightSalePNR in PermissionOutboundPaymentNotReceive_ByLicenseId_List">
                        @*<td class="table-color"><a href="~/PermissionsOutbound/PermissionsOutbound/PermissionsOutboundPaymentTagging?ProductLicenseId={{RightSalePNR.Id}}"> <span ng-bind="(RightSalePNR.ProductLicensecode)"></span> </a></td>*@

                        <td class="table-color"><a href="~/PermissionsOutbound/PermissionsOutbound/PermissionsOutboundPaymentTagging?ProductLicenseId={{RightSalePNR.Id}}&OutboundId={{RightSalePNR.PermissionsOutboundId}}&ProuductId={{RightSalePNR.ProductId}}"> <span ng-bind="(RightSalePNR.ProductLicensecode)"></span> </a></td>


                        <td class="table-color"><a href="~/Product/ProductMaster/ProductDetailsView?Id={{RightSalePNR.ProductId}}"> <span ng-bind="(RightSalePNR.ProductCode)"></span></a> </td>

                        <td><span ng-bind="(RightSalePNR.OrganizationName)"></span> </td>
                        <td><span ng-bind="(RightSalePNR.ExecutiveName)"></span> </td>
                        <td><span ng-bind="(RightSalePNR.EntryDate)"></span> </td>

                        <td> <span ng-bind="(RightSalePNR.PermissionsOutboundCode)"></span>  </td>
                        <td><span ng-bind="(RightSalePNR.WorkingProduct)"></span>   <span style="font-size:11px;">  <br />  <span ng-bind="(RightSalePNR.WorkingSubProduct)"></span></span></td>
                        <td>License</td>
                    </tr>
                </table>
                <a href="~/PaymentTagging/PaymentTagging/PaymentTaggingSearch?For=PermissionOutbound&From=Dashboard" class="moreDash"> View More</a>

            </div>
            <div ng-show="!PermissionOutboundPaymentNotReceive_ByContractId_List.length && !PermissionOutboundPaymentNotReceive_ByLicenseId_List.length">No result found</div>
        </div>
    </div>
</div>





