
<style type="text/css">
    .table-color a {
        color: #ee5941;
    }
</style>
<div class="portlet box green">
    <div class="portlet-title">
        <div class="caption">
            Right Sales Contracts Expiring Within 3 Months - License
        </div>
        <div class="tools">
            <a href="javascript:;" class="collapse"> </a>
        </div>
    </div>
    <div class="portlet-body">
        <div class="table-responsive">
            <div ng-hide="!RightSaleContractExpiring_List_License.length">
                <table class="table table-bordered table-striped">
                    <tr>
                        <th> Product Code </th>
                        <th> ISBN </th>
                        <th> Working Product</th>
                        <th> Author </th>
                        <th> Expiry Date </th>
                        <th> Territorial Rights </th>
                    </tr>

                    <tr ng-repeat="RSP in RightSaleContractExpiring_List_License">
                        <td class="table-color"><a href="~/RightsSelling/RightsSelling/RightsSellingUpdate?Id={{RSP.ProductId}}&type=P{{RSP.ProductId}}&RightsSellingId={{RSP.RightsSellingID}}"> <span ng-bind="(Product.ProductCode)"></span> </a></td>
                        <td><span ng-bind="(Product.ISBN)"></span>   </td>
                        <td> <span ng-bind="(Product.WorkingProduct)"></span>   </td>
                        <td> <span ng-bind="(Product.AuthorName)"></span>  </td>
                        <td> <span ng-bind="(Product.Expirydate)"></span> </td>
                        <td><span ng-bind="(Product.Territoryrights)"></span>  </td>
                    </tr>

                </table>
                <a href="javascript:void(0);" ng-click="RightSaleContractExpire_submitForm();" class="moreDash">View More</a>
            </div>
            <div ng-hide="RightSaleContractExpiring_List_License.length">No result found</div>
        </div>
    </div>
</div>



