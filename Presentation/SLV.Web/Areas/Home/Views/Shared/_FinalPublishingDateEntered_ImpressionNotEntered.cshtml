
<style type="text/css">
    .table-color a {
        color: #ee5941;
    }
</style>
<div class="portlet box green">
    <div class="portlet-title">
        <div class="caption">
            Final Publishing Date Is Entered - Impression Not Entered - License
        </div>
        <div class="tools">
            <a href="javascript:;" class="collapse"> </a>
        </div>
    </div>
    <div class="portlet-body">
        <div class="table-responsive">
            <div ng-hide="!ImpressionNotEntered_List.length">
                <table class="table table-bordered table-striped">
                    <tr>
                        <th> Product Code </th>
                        <th>Product License Code</th>
                        <th> ISBN </th>
                        <th> Working Product</th>
                        <th> Author </th>
                    </tr>

                    <tr ng-repeat="Product in ImpressionNotEntered_List">
                        <td class="table-color"><a href="~/Product/ProductLicense/ImpressionEntry?ProductId={{Product.Id}}&LicenseId={{Product.ProductLicenseId}}"> <span ng-bind="(Product.ProductLicensecode)"></span> </a></td>
                        <td><span ng-bind="(Product.ProductCode)"></span> </td>
                        <td><span ng-bind="(Product.ISBN)"></span>   </td>
                        <td><span ng-bind="(Product.WorkingProduct)"></span>  </td>
                        <td><span ng-bind="(Product.AuthorName)"></span>  </td>
                    </tr>
                </table>
                <a href="~/Product/ProductLicense/ImpressionSearch" class="moreDash">View More</a>
            </div>
            <div ng-hide="ImpressionNotEntered_List.length">No result found</div>
        </div>
    </div>
</div>



