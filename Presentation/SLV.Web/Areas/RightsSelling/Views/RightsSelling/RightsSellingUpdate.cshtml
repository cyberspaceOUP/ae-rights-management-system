                            <div class="form-body">
                                @Html.Partial("~/Areas/RightsSelling/Views/Shared/_RoyaltySlabView.cshtml")
                            </div>
                        </div>
                    </div>
                </div>

            </div>

           

            <div class="col-md-12" ng-if="(('@TempData["User"]'=='ad' || '@TempData["User"]'=='sa'))">
