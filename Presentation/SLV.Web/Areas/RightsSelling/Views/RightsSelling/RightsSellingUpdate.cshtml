                endDate: TodayDate



            })
    });
    $(function () {
      
        setTimeout(function () { renderFileUpload(); }, 5000);
        
        $(document).on('click', '.RoyaltySlabnotAdd', function () {
          
            if (flag == 0) {
                angular.element(document.getElementById('angularid')).scope().addroyalslabbyJquery($(this));
                flag = 1;
            }
            setTimeout(function () { flag = 0; }, 200);
        });

        $(document).on('click', '.RoyaltySlabnotRemove', function () {

            angular.element(document.getElementById('angularid')).scope().removeroyalslab($(this));
        });
    });

    function renderFileUpload() {
        $.ajax({
            cache: false,
            type: "GET",
            contentType: 'application/html; charset=utf-8',
            url: GlobalredirectPath + "/Master/Master/MultipleFileUpload",
            data: {},
            datatype: 'html',
            success: function (result) {
                $("#fileuploadcontrol").html(result);

            },
            error: function (xhr, ajaxOptions, thrownError) {

            }
        });

    }



    setTimeout(function () {

        if ($("#hid_RightsSellingId").val() != "" ) {
            $('.AuthorBoxAddendum').each(function () {

                var obj = $(this);
                $(obj).find(".RoyaltySlab").find("tbody tr").each(function (index) {

                    if (index == $(obj).find(".RoyaltySlab").find("tbody tr").length - 1) {
                        $(this).find(".RoyaltySlabnotRemove").css("display", "none");
                    }
                    else {
                        $(this).find(".RoyaltySlabnotAdd").css("display", "none");
                    }

                });

            });
        }
    }, 1900);

    function RoyaltySlab(obj) {
        angular.element(document.getElementById('angularid')).scope().ValidateRoyaltySlabInsert($(obj));
    };

</script>

