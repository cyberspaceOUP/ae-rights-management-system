        </div>
    </div>
</div>


<script type="text/javascript">

    $(document).ready(function () {

        $('.PermissionsInboundDate')
        .datepicker({
            format: "dd/mm/yyyy",
            showButtonPanel: true,
            autoclose: true,
            //  startDate: date,
            //  endDate: TodayDate
        })
    });

    $(function () {
        renderFileUploadOther();
        renderFileUpload();
    });


    function renderFileUploadOther() {

        $.ajax({
            cache: false,
            type: "GET",
            contentType: 'application/html; charset=utf-8',

            url: GlobalredirectPath + "/Master/Master/MultipleFileUploadPendingRequest",
            data: {},
            datatype: 'html',
            success: function (result) {

                $("#fileuploadcontrol").html(result);
                $("#fileuploadcontrol").find("input[type=hidden][id=hid_foldername]").val("InboundDocument");


            },
            error: function (xhr, ajaxOptions, thrownError) {

            }
        });

    }
    function Filldate(obj) {
        var obj = $(obj);
        angular.element(document.getElementById('angularid')).scope().SetDateValue(obj);
    }
    function FillExpiry(obj) {
        var obj = $(obj);
        angular.element(document.getElementById('angularid')).scope().FillExpiry(obj);
    }
    setTimeout(function () { $("select[name*=Currency]").val(6) }, 2000);


    function renderFileUpload() {
        $.ajax({
            cache: false,
            type: "GET",
            contentType: 'application/html; charset=utf-8',
            url: GlobalredirectPath + "/Master/Master/MultipleFileUpload",
            data: {},
            datatype: 'html',
            success: function (result) {
                $(".uploadFileMutple").html(result);

            },
            error: function (xhr, ajaxOptions, thrownError) {

            }
        });

    }

    ////--manage multiple product for (Copy Existing Permissions Of Other Product)
    $('#btnSubmitInboundProduct').prop('disabled', 'disabled');
    var count_productId = [];
    $('#btnShowInboundPopup').click(function () {
        $('#btnSubmitInboundProduct').prop('disabled', 'disabled');
        count_productId = [];
    })

    function fnSelectProduct(data) {
        if (data.checked == true) {
            count_productId.push(data.value);
        }
        else {
            //count_productId.pop(data.value);
            var index = count_productId.indexOf(data.value);
            if (index !== -1) {
                count_productId.splice(index, 1);
            }
        }

        if (count_productId.length > 0) {
            $('#btnSubmitInboundProduct').removeAttr('disabled');
        }
        else {
            $('#btnSubmitInboundProduct').prop('disabled', 'disabled');
        }
    }

    $('#btnSubmitInboundProduct').click(function () {
        var finalId = '';
        if (count_productId.length > 0) {
            finalId = count_productId.join(',');
        }
        $('#hid_CkeckedProduct').val(finalId);
    });
    ////--end manage multiple product for (Copy Existing Permissions Of Other Product)
</script>



