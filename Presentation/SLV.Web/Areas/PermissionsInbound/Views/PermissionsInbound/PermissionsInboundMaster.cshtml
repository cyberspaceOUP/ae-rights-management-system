                                                <div class="col-md-7">
                                                    <div class="form-group">
                                                        <label class="radio-inline" style="padding-top: 0px !important;">
                                                            <input type="radio" name="PrintRunGrantedForPrint_{{$index}}" ng-model="PrintRunGrantedForPrint[$index]" value="Unrestricted"> Unrestricted
                                                        </label>
                                                        <label class="radio-inline" style="padding-top: 0px !important;">
                                                            <input type="radio" name="PrintRunGrantedForPrint_{{$index}}" ng-model="PrintRunGrantedForPrint[$index]" value="Number"> Number
                                                        </label>
                                                        <div ng-class="{ 'has-error' : userForm.PrintRunGrantedForPrint_{{$index}} .$invalid && (userForm.PrintRunGrantedForPrint_{{$index}} .$dirty || submitted)}">
                                                            <div class="help-block">
                                                                <p ng-show="userForm.PrintRunGrantedForPrint_{{$index}} .$error.required && (userForm.PrintRunGrantedForPrint_{{$index}}.$dirty || submitted)" class="help-block">Please select Print Run Granted</p>
                                                            </div>
                                                        </div>
                                                    </div>

                                                </div>
