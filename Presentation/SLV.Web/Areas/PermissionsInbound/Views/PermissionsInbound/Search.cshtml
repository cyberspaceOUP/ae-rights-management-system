<script type="text/javascript">

    $(document).ready(function () {
        $('.PermissionsInboundDate')
                .datepicker({
                    format: "dd/mm/yyyy",
                    showButtonPanel: true,
                    autoclose: true,

                })
    });
    function Filldate(obj) {
        var obj = $(obj);
        angular.element(document.getElementById('angularid')).scope().SetDateValue(obj);
    }
    function FillExpiry(obj) {
        var obj = $(obj);
        angular.element(document.getElementById('angularid')).scope().FillExpiry(obj);
    }

    function fireMe(obj) {
        var length = $(".customizeLink").length;
        for (i = 0; i < $(".customizeLink").length; i++) {
            $($(".customizeLink")[i]).attr("sequence", i);
        }
        angular.element(document.getElementById('angularid')).scope().DisplayDiv(parseInt($(obj).attr("sequence")) + 1);
    };

</script>
