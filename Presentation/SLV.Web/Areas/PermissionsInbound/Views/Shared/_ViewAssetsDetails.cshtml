                                    <div class="col-md-12">
                                        <label class="control-label col-md-5 col-xs-6 col-sm-6">Acknowledgement line</label>
                                        <div class="col-md-7 col-xs-6 col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">{{AcknowledgementlineView}} </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-12">
                                        <label class="control-label col-md-5 col-xs-6 col-sm-6">Remarks</label>
                                        <div class="col-md-7 col-xs-6 col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">{{InboundRemarks}} </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="col-md-12" ng-if="(PermissionInboundOthersRightsDataList.length)">
            <div class="portlet box green">
                <div class="portlet-title">
                    <div class="caption">
                        Other Details
                    </div>
                    <div class="tools">
                        <a href="javascript:;" class="collapse" data-original-title="" title=""> </a>
                    </div>
                </div>
                <div class="portlet-body clearfix">
                    <div class="col-md-12">
                        <div class="row">
                            <table class="table table-striped table-bordered table-hover dataTable no-footer SeriesDetail" id="TblOwnerList">
                                <thead class="flip-content">
                                    <tr>
                                        <th>S.No.</th>
                                        <th>Rights Types</th>
                                        <th>Status</th>
                                        <th>Granted For </th>
                                        <th>Number</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr ng-repeat="M in PermissionInboundOthersRightsDataList">
                                        <td>{{$index +1}}</td>
                                        <td>{{M.RightsName}}</td>
                                        <td>{{M.status}}</td>
                                        <td>{{M.RunGranted}}</td>
                                        <td>{{M.Number}}</td>
                                    </tr>
                                </tbody>
                            </table>

                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="col-md-12" ng-if="(OtherContractDateRequestList.length)">
            <div class="portlet box green">
                <div class="portlet-title">
                    <div class="caption">
                        Date Request Details
                    </div>
                    <div class="tools">
                        <a href="javascript:;" class="collapse" data-original-title="" title=""> </a>
                    </div>
                </div>
                <div class="portlet-body clearfix">
                    <div class="col-md-12">
                        <div class="row">

                            <table class="table table-striped table-bordered table-hover dataTable no-footer SeriesDetail" id="TblOwnerList">
                                <thead class="flip-content">
                                    <tr>
                                        <th>S.No.</th>
                                        <th>Date of Request</th>
                                        <th>Date</th>
                                    </tr>
                                </thead>
                                <tbody>

                                    <tr ng-repeat="M in OtherContractDateRequestList">
                                        <td>{{$index +1}}</td>
                                        <td>{{M.dateOf}}</td>
                                        <td>{{M.dateValue}}</td>
                                    </tr>
                                </tbody>
                            </table>

                        </div>
                    </div>
                </div>
            </div>
        </div>

        </div>
    </div>





