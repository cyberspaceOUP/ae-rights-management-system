<div class="modal-body">
    <button type="button" class="close" data-dismiss="modal" style="padding:20px; padding-top :20px;">&times;</button>

    <div class="row panel">
        <div class="col-md-12">
            <div class="portlet box green">
                <div class="portlet-title">
                    <div class="caption">
                        Assets Details
                    </div>
                    <div class="tools">
                        <a href="javascript:void(0);" class="collapse"> </a>
                    </div>
                </div>
                <div class="portlet-body form">
                    <div class="form-body">
                        <div class="row">
                            <div class="col-md-6 col-xs-6 col-sm-6">
                                <div class=" row">
                                    <div class="col-md-12">
                                        <label class="control-label col-md-5 col-xs-6 col-sm-6">	Status</label>
                                        <div class="col-md-7 col-xs-6 col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">{{StatusView}} </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class=" row">
                                    <div class="col-md-12">
                                        <label class="control-label col-md-5 col-xs-6 col-sm-6">Asset Sub-Type</label>
                                        <div class="col-md-7 col-xs-6 col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">{{AssetSubTypeView}} </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class=" row">
                                    <div class="col-md-12">
                                        <label class="control-label col-md-5 col-xs-6 col-sm-6">Asset Description </label>
                                        <div class="col-md-7 col-xs-6 col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">{{AssetDescriptionView}} </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class=" row">
                                    <div class="col-md-12">
                                        <label class="control-label col-md-5 col-xs-6 col-sm-6">Extent </label>
                                        <div class="col-md-7 col-xs-6 col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">{{ExtentView}} </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class=" row">
                                    <div class="col-md-12">
                                        <label class="control-label col-md-5 col-xs-6 col-sm-6">Gratis copies to be sent</label>
                                        <div class="col-md-7 col-xs-6 col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">{{GratiscopytobesentView}} </label>
                                            </div>

                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-12">
                                        <label class="control-label col-md-5 col-xs-6 col-sm-6">Number of copies </label>
                                        <div class="col-md-7 col-xs-6 col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">{{NoofcopyView}} </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class=" row">
                                    <div class="col-md-12">
                                        <label class="control-label col-md-5 col-xs-6 col-sm-6">Original Source </label>
                                        <div class="col-md-7 col-xs-6 col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">{{OriginalSourceView}} </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class=" row">
                                    <div class="col-md-12">
                                        <label class="control-label col-md-5 col-xs-6 col-sm-6">Restriction </label>
                                        <div class="col-md-7 col-xs-6 col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">{{RestrictionView}} </label>
                                            </div>

                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-md-6 col-xs-6 col-sm-6">
                                <div class=" row">
                                    <div class="col-md-12">
                                        <label class="control-label col-md-5 col-xs-6 col-sm-6">Sub-Licensing</label>
                                        <div class="col-md-7 col-xs-6 col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">{{SubLicensingView}} </label>
                                            </div>

                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-12">
                                        <label class="control-label col-md-5 col-xs-6 col-sm-6">Fee  </label>
                                        <div class="col-md-7 col-xs-6 col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">{{FeeView}} </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-12">
                                        <label class="control-label col-md-5 col-xs-6 col-sm-6">Currency</label>
                                        <div class="col-md-7 col-xs-6 col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">{{CurrencyView}} </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="row">
                                    <div class="col-md-12">
                                        <label class="control-label col-md-5 col-xs-6 col-sm-6">Territory</label>
                                        <div class="col-md-7 col-xs-6 col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">{{TerritoryRightsView}} </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>


                                <div class="row">
                                    <div class="col-md-12">
                                        <label class="control-label col-md-5 col-xs-6 col-sm-6">Invoice Number   </label>
                                        <div class="col-md-7 col-xs-6 col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">{{InvoiceNumberView}} </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-12">
                                        <label class="control-label col-md-5 col-xs-6 col-sm-6">Invoice Value   </label>
                                        <div class="col-md-7 col-xs-6 col-sm-6">
                                            <div class="form-group">
                                                <label class="control-label">{{InvoiceValueView}} </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>
