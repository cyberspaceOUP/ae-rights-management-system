<script src="~/js/PermissionsInbound/UpdateMoreCopyrightHolder.js"></script>
<script src="~/js/PermissionsInbound/PermissionInboundOthersView.js"></script>
@*<script src="~/js/PermissionsInbound/InsertAddMoreMoreCopyrightHolder.js"></script>*@

<script src="~/js/PermissionsInbound/ImageVideoBankView.js"></script>

<div ng-show="true" @*ng-show="PermissionInboundOthersView"*@>
    
    <div class="col-md-12" ng-if="OtherContractList.length>0">
        <div class="portlet box green">
            <div class="portlet-title">
                <div class="caption">
                    Copyright holder Details
                </div>
                <div class="tools">
                    <a href="javascript:;" class="collapse"> </a>
                </div>
            </div>
            <div class="portlet-body clearfix">
                <div class="col-md-12">
                    <div class="row">


                        <table class="table table-striped table-bordered table-hover dataTable no-footer SeriesDetail" id="TblOwnerList">
                            <thead class="flip-content">
                                <tr>
                                    <th>S.No.</th>
                                    <th>Update</th>
                                    @*<th>Asset Sub-Type</th>
                                    <th>Asset Description</th>
                                    <th>Copyright Holder</th>
                                    <th>Contact Person</th>
                                    <th>Address </th>
                                    <th>Mobile</th>*@
                                    <th>Status </th>

                                    <th>Asset Sub-Type</th>
                                    <th>Asset Description</th>
                                    <th>Copyright Holder</th>

                                    <th>Print Rights</th>
                                    <th>Electronic Rights</th>
                                    <th>E-Book Rights</th>
                                    <th>All Future Rights Granted</th>

                                    <th>Permission Expiry date</th>
                                    @*<th>Quantity</th>*@

                                </tr>
                            </thead>
                            <tbody>

                                <tr ng-repeat="M in OtherContractList">
                                    <td>{{$index +1}}</td>
                                    <td>
                                        <input type="radio" name="Check_OtherUpdate" ng-model="Check_OtherUpdate" ng-click="ViewCopyRightHolderById(M.CopyRightHolderId,M.InboundOthersId, M.CopyRightHolder, M.CopyRightHolderMasterId[0].CopyRightHolderMasterId)" data-toggle="modal" data-target="#UpdateOther" value="Check_OtherUpdate">
                                        <input type="hidden" ng-model="hid_CopyRightHolderById" name="hid_CopyRightHolderById" id="hid_CopyRightHolderById" />
                                    </td>

                                    @*<td>{{M.AssetName}} </td>
                                    <td>{{M.AssetDescription}} </td>

                                    <td>{{M.CopyRightHolderName}}</td>
                                    <td>{{M.ContactPerson}} </td>
                                    <td>{{M.Address}} {{M.Pincode !=null?"-":""}} {{M.Pincode}}</td>
                                    <td>{{M.Mobile}} </td>*@


                                    <td> <span ng-bind="(M.StatusName)"></span></td>

                                    <td> <span ng-bind="(M.AssetSubType)"></span></td>
                                    


                                    <td >

                                        <span ng-bind="(M.AssestDescription)"> </span>


                                    </td>



                                   


                                    <td>

                                        <span ng-bind="M.CopyRightHolder"></span>

                                    </td>


                                    <td ng-repeat="obj  in M.Rights"> <span ng-bind="(obj.Rightsvalue)"></span> <br />   <span style="font-size:10px;" ng-bind="(obj.Number)"></span>  </td>
                                    
                                    @*<td ng-repeat="obj  in M.Rights"> <span ng-bind="(obj.Rightsvalue)"></span> </td>*@

                                    @*<td ng-if="M.TotalQty !=0"> <span ng-bind="(M.TotalQty)"></span> </td>

                                    <td ng-if="M.TotalQty ==0"> </td>*@

                                    <td>

                                        <span ng-bind="M.PermissionExpirydate | date : 'dd/MM/yyyy'"></span>

                                    </td>

                                </tr>



                            </tbody>
                        </table>


                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="col-md-12" ng-show="AddMoreOther">
        <div class="portlet-title">
            <div class="caption">

                <a href="javasript:void(0);" ng-click="ViewCopyRightHolderByIdValue()" data-toggle="modal" data-target="#UpdateOther" style="float: right; color: #ee5941;">Add More Others Details</a>
            </div>

        </div>
    </div>
    
    <div class="clearfix">
        &nbsp;
    </div>
  

</div>
<div class="modal fade bs-example-modal-lg clickData" id="UpdateOther" tabindex=" -1" role="dialog" aria-labelledby="myModalLabel">
    <div class="modal-dialog " role="document" style="width:90%;">

        @Html.Partial("~/Areas/PermissionsInbound/Views/Shared/_UpdateCopyrightHolder.cshtml")

    </div>
</div>

