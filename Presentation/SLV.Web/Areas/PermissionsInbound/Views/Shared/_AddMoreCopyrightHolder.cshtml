
<div class="portlet box green">
    <div class="portlet-title">
        <div class="caption">
            Add More Third party Assets
        </div>
        <div class="tools">
            <a href="javascript:;" class="collapse"> </a>

        </div>
    </div>
    <div class="portlet-body">
        <div class="row">
            <div class="col-md-6">
                <label class="control-label col-md-5">CopyRight Holder&nbsp;  <span class="red">*</span></label>
                <div class="col-md-7">
                    <div class="form-group" ng-class="{ 'has-error': (submitted ||userForm.CopyRightHolder.$dirty) && userForm.CopyRightHolder.$invalid } ">
                        <select class="form-control" name="CopyRightHolder" id="CopyRightHolder" onchange="slideUpDown($(this))" ng-model="PermissionsInboundModel.CopyRightHolder" ng-change="onCopyRightHolder()" required>
                            <option value="">Please Select</option>
                            <option ng-repeat="div in newCpyHlderList" ng-model="CopyRightHolder" value="{{div.Id}}">{{div.CopyRightHolderName}}</option>
                        </select>
                        <div class="help-block">
                            <p ng-show="userForm.CopyRightHolder.$error.required && (userForm.CopyRightHolder.$dirty || submitted)">Please select Copyright holder</p>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-md-6 fadeInout" style="display:none;">
                <label class="control-label col-md-5">Contact Person </label>
                <div class="col-md-7">
                    <label class="control-label">{{ContactPerson}}</label>
                </div>
            </div>
        </div>
        <div class="row slideshow">
            <div class="col-md-6">
                <div class=" row">
                    <div class="col-md-12">
                        <label class="control-label col-md-5">	Status&nbsp;<span class="red">*</span></label>
                        <div class="col-md-7">
                            <div class="form-group" ng-class="{ 'has-error': (submitted ||userForm.Status.$dirty) && userForm.Status.$invalid } ">
                                <select class="form-control" name="Status" id="Status" ng-change="MinimumValidationRequired($event)" ng-model="PermissionsInboundModel.Status" required>
                                    <option value="">Please Select</option>
                                    <option ng-repeat="div in StatusList" value="{{div.Id}}">{{div.Status}}</option>
                                </select>
                                <div class="help-block">
                                    <p ng-show="userForm.Status.$error.required && (userForm.Status.$dirty || submitted)">Please select Status</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class=" row">
                    <div class="col-md-12">
                        <label class="control-label col-md-5">Asset Sub-Type&nbsp;<span ng-show="OtherContractStatus" class="red">*</span></label>
                        <div class="col-md-7">
                            <div class="form-group" ng-class="{ 'has-error': (submitted ||userForm.AssetSubType.$dirty) && userForm.AssetSubType.$invalid } ">
                                <select class="form-control" name="AssetSubType" id="AssetSubType" ng-model="PermissionsInboundModel.AssetSubType" ng-required="OtherContractStatus==true">
                                    <option value="">Please Select</option>
                                    <option ng-repeat="div in AssetSubTypeList" ng-model="AssetSubType" value="{{div.Id}}">{{div.AssetName}}</option>
                                </select>
                                <div class="help-block">
                                    <p ng-show="userForm.AssetSubType.$error.required && (userForm.AssetSubType.$dirty || submitted)">Please select Asset Sub-Type</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>


                <div class=" row">
                    <div class="col-md-12">
                        <label class="control-label col-md-5">Asset Description &nbsp;<span ng-show="OtherContractStatus" class="red">*</span></label>
                        <div class="col-md-7">
                            <div class="form-group" ng-class="{ 'has-error': (submitted ||userForm.AssetDescription.$dirty) && userForm.AssetDescription.$invalid } ">
                                <textarea class="form-control" ng-model="PermissionsInboundModel.AssetDescription" name="AssetDescription" placeholder="Asset Description" ng-required="OtherContractStatus==true "></textarea>
                                <div class="help-block">
                                    <p ng-show="userForm.AssetDescription.$error.required && (userForm.AssetDescription.$dirty || submitted)">Please enter Asset Description</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>


            </div>
            <div class="col-md-6">
                <div class=" row">
                    <div class="col-md-12">
                        <label class="control-label col-md-5">Extent </label>
                        <div class="col-md-7">
                            <div class="form-group" ng-class="{ 'has-error': (submitted ||userForm.Extent.$dirty) && userForm.Extent.$invalid } ">
                                <input type="text" class="form-control" ng-model="PermissionsInboundModel.Extent" name="Extent" placeholder="Extent" />
                                <div class="help-block">
                                    <p ng-show="userForm.Extent.$error.required && (userForm.Extent.$dirty || submitted)">Please enter Extent</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class=" row">
                    <div class="col-md-12">
                        <label class="control-label col-md-5">Gratis copies to be sent&nbsp;<span ng-show="OtherContractStatus" class="red">*</span></label>
                        <div class="col-md-7">
                            <div class="form-group">
                                <label class="radio-inline" style="padding-top: 0px !important;">
                                    <input type="radio" name="Gratiscopytobesent " ng-model="PermissionsInboundModel.Gratiscopytobesent" value="Yes" ng-required="OtherContractStatus==true "> Yes
                                </label>
                                <label class="radio-inline" style="padding-top: 0px !important;">
                                    <input type="radio" name="Gratiscopytobesent " ng-model="PermissionsInboundModel.Gratiscopytobesent" value="No" ng-required="OtherContractStatus==true "> No
                                </label>
                                <div ng-class="{ 'has-error' : userForm.Gratiscopytobesent .$invalid && (userForm.Gratiscopytobesent .$dirty || submitted)}">
                                    <div class="help-block">
                                        <p ng-show="userForm.Gratiscopytobesent .$error.required && (userForm.Gratiscopytobesent.$dirty || submitted)" class="help-block">Please select Gratis copies to be sent</p>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-12">
                        <label class="control-label col-md-5">Number of copies&nbsp;<span class="red" ng-show="(PermissionsInboundModel.Gratiscopytobesent == 'Yes')">*</span> </label>
                        <div class="col-md-7">
                            <div class="form-group" ng-class="{ 'has-error': ( submitted ||userForm.Noofcopy.$dirty) && userForm.Noofcopy.$invalid } ">

                                <input type="number" name="Noofcopy" class="form-control" ng-required="(PermissionsInboundModel.Gratiscopytobesent == 'Yes')" ng-model="PermissionsInboundModel.Noofcopy" placeholder="Number of copies">


                                <div class="help-block">
                                    <p ng-show="userForm.Noofcopy .$error.required && (userForm.Noofcopy .$dirty || submitted)" class="help-block red">Please enter Number of copies</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class=" row">
                    <div class="col-md-12">
                        <label class="control-label col-md-5">Original Source </label>
                        <div class="col-md-7">
                            <div class="form-group" ng-class="{ 'has-error': (submitted ||userForm.OriginalSource.$dirty) && userForm.OriginalSource.$invalid } ">
                                <input type="text" class="form-control" ng-model="PermissionsInboundModel.OriginalSource" name="OriginalSource" placeholder="Original Source" />
                                <div class="help-block">
                                    <p ng-show="userForm.OriginalSource.$error.required && (userForm.OriginalSource.$dirty || submitted)">Please enter Original Source</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
     
            <div class="clearfix">&nbsp;</div>
            <div class="col-md-6">
                <div class=" row">
                    <div class="col-md-12">
                        <label class="control-label col-md-5">Restriction </label>
                        <div class="col-md-7">
                            <div class="form-group" ng-class="{'has:error':(submittted || userForm.Restriction.$dirty) && userForm.Restriction.$invalid}">
                                <input type="text" class="form-control" name="Restriction" ng-model="PermissionsInboundModel.Restriction" id="Restriction" placeholder="Restriction" />
                                <div class="help-block">
                                    <p ng-show="userForm.Restriction.$error.required &&(userForm.Restriction.$dirty || submitted )">Please enter Restriction</p>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>


                <div class="row rightsObject" ng-if="OtherRightsMasterList.length>0" ng-repeat="div in OtherRightsMasterList" rightsid="{{div.Id}}">
                    <div class="col-md-12">
                        <label class="control-label col-md-5">{{div.RightsName}}&nbsp;  <span class="red" ng-show="OtherContractStatus" ng-if="div.Id!=4">*</span></label>
                        <div class="col-md-7">
                            <div class="form-group" ng-class="{ 'has-error': ( submitted ||userForm.PrintRights_{{$index}}.$dirty) && userForm.PrintRights_{{$index}}.$invalid } ">
                                <label class="radio-inline" style="padding-top: 0px !important;">
                                    <input type="radio" name="PrintRights_{{$index}}" ng-model="PermissionsInboundRightsModel[$index]" value="Yes" ng-required="OtherContractStatus==true "> Yes
                                </label>
                                <label class="radio-inline" style="padding-top: 0px !important;">
                                    <input type="radio" name="PrintRights_{{$index}}" ng-model="PermissionsInboundRightsModel[$index]" value="No" ng-required="OtherContractStatus==true "> No
                                </label>
                                <div ng-class="{ 'has-error' : userForm.PrintRights_{{$index}} .$invalid && (userForm.PrintRights_{{$index}} .$dirty || submitted)}">
                                    <div class="help-block">
                                        <p ng-show="userForm.PrintRights_{{$index}} .$error.required && (userForm.PrintRights_{{$index}}.$dirty || submitted)" class="help-block">Please select {{div.RightsName}}</p>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                    <div class="col-md-12" ng-if="div.Id!=4" ng-show="(PermissionsInboundRightsModel[$index] == 'Yes') && div.Id!=4">
                        <label class="control-label col-md-5">{{div.RightsName}} Granted For&nbsp;<span ng-show="(PermissionsInboundModel.PrintRights == 'Yes')" class="red">*</span>   </label>
                        <div class="col-md-7">
                            <div class="form-group">
                                <label class="radio-inline" style="padding-top: 0px !important;">
                                    <input type="radio" name="PrintRunGrantedForPrint_{{$index}}" ng-model="PrintRunGrantedForPrint[$index]" value="Unrestricted"> Unrestricted
                                </label>
                                <label class="radio-inline" style="padding-top: 0px !important;">
                                    <input type="radio" name="PrintRunGrantedForPrint_{{$index}}" ng-model="PrintRunGrantedForPrint[$index]" value="Number"> Number
                                </label>
                                <div ng-class="{ 'has-error' : userForm.PrintRunGrantedForPrint_{{$index}} .$invalid && (userForm.PrintRunGrantedForPrint_{{$index}} .$dirty || submitted)}">
                                    <div class="help-block">
                                        <p ng-show="userForm.PrintRunGrantedForPrint_{{$index}} .$error.required && (userForm.PrintRunGrantedForPrint_{{$index}}.$dirty || submitted)" class="help-block">Please select Print Run Granted</p>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                    <div class="col-md-12" ng-if="div.Id!=4" ng-show="(PermissionsInboundRightsModel[$index] == 'Yes')">
                        <label class="control-label col-md-5">Number &nbsp; <span ng-show="PrintRunGrantedForPrint[$index] == 'Number' && OtherContractStatus " class="red">*</span> </label>
                        <div class="col-md-7">
                            <div class="form-group" ng-class="{ 'has-error': (submitted ||userForm.NumberPrint_{{index}}.$dirty) && userForm.NumberPrint_{{index}}.$invalid } ">
                                <input type="number" class="form-control" min="0" ng-model="NumberPrint[$index]" name="NumberPrint_{{index}}" placeholder="Number" ng-required="PrintRunGrantedForPrint[$index] == 'Number' && OtherContractStatus==true  " />
                                <div class="help-block">
                                    <p ng-show="userForm.NumberPrint_{{index}}.$error.required && (userForm.NumberPrint_{{index}}.$dirty || submitted)">Please enter Number</p>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>



                <div class=" row">
                    <div class="col-md-12">
                        <label class="control-label col-md-5">Sub-Licensing</label>
                        <div class="col-md-7">
                            <div class="form-group">
                                <label class="radio-inline" style="padding-top: 0px !important;">
                                    <input type="radio" name="SubLicensing" ng-model="PermissionsInboundModel.SubLicensing" value="Yes"> Yes
                                </label>
                                <label class="radio-inline" style="padding-top: 0px !important;">
                                    <input type="radio" name="SubLicensing" ng-model="PermissionsInboundModel.SubLicensing" value="No"> No
                                </label>
                                <div ng-class="{ 'has-error' : userForm.SubLicensing .$invalid && (userForm.SubLicensing.$dirty || submitted)}">
                                    <div class="help-block">
                                        <p ng-show="userForm.SubLicensing .$error.required && (userForm.SubLicensing.$dirty || submitted)" class="help-block">Please select Sub-Licensing</p>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <label class="control-label col-md-5">Fee  </label>
                        <div class="col-md-7">
                            <div class="form-group" ng-class="{ 'has-error': (submitted ||userForm.Fee.$dirty) && userForm.Fee.$invalid } ">
                                <input type="number" class="form-control" ng-model="PermissionsInboundModel.Fee" name="Fee" placeholder="Fee" />
                                <div class="help-block">
                                    <p ng-show="userForm.Fee.$error.required && (userForm.Fee.$dirty || submitted)">Please enter Fee</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>


                <div class="row">
                    <div class="col-md-12">
                        <label class="control-label col-md-5">Currency</label>
                        <div class="col-md-7">
                            <div class="form-group" ng-class="{ 'has-error': ( submitted ||userForm.Currency.$dirty) && userForm.Currency.$invalid } ">
                                <select class="form-control" name="Currency" ng-model="PermissionsInboundModel.CurrencyValue">
                                    <option value="">--Please Select--</option>
                                    <option ng-repeat="Currency in CurrencyList" ng-selected="Currency.Id == CurrencyId" value="{{Currency.Id}}">{{Currency.CurrencyName}}</option>
                                </select>
                            </div>
                        </div>
                    </div>
                </div>


            </div>
            <div class="col-md-6">


                <div class="row">
                    <div class="col-md-12">
                        <label class="control-label col-md-5">Invoice Number   </label>
                        <div class="col-md-7">
                            <div class="form-group" ng-class="{ 'has-error': (submitted ||userForm.InvoiceNumber.$dirty) && userForm.InvoiceNumber.$invalid } ">
                                <input type="text" class="form-control" ng-model="PermissionsInboundModel.InvoiceNumber" name="InvoiceNumber" placeholder="Invoice Number" />
                                <div class="help-block">
                                    <p ng-show="userForm.InvoiceNumber.$error.required && (userForm.InvoiceNumber.$dirty || submitted)">Please enter Invoice Number</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-12">
                        <label class="control-label col-md-5">Invoice Value   </label>
                        <div class="col-md-7">
                            <div class="form-group" ng-class="{ 'has-error': (submitted ||userForm.InvoiceValue.$dirty) && userForm.InvoiceValue.$invalid } ">
                                <input type="number" class="form-control" ng-model="PermissionsInboundModel.InvoiceValue" name="InvoiceValue" placeholder="Invoice Value" maxlength="8" />
                                <div class="help-block">
                                    <p ng-show="userForm.InvoiceValue.$error.required && (userForm.InvoiceValue.$dirty || submitted)">Please enter Invoice Value</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>


                <div class="row">
                    <div class="col-md-12">
                        <label class="control-label col-md-5">Permission Expiry date</label>
                        <div class="col-md-7">
                            <div class="form-group date" ng-class="{ 'has-error': ( submitted ||userForm.PermissionExpirydate.$dirty) && userForm.PermissionExpirydate.$invalid } ">
                                <div class="input-group input-append date PermissionsInboundDate">
                                    <input type="text" id="PermissionExpirydate" onchange="FillExpiry(this)" name="PermissionExpirydate" class="form-control" ng-model="PermissionsOutboundModel.PermissionExpirydate" placeholder="Permission Expiry date">
                                    <span class="input-group-addon add-on"><span class="glyphicon glyphicon-calendar"></span></span>
                                </div>
                                <div class="help-block">
                                    <p ng-show="userForm.PermissionExpirydate .$error.required && (userForm.PermissionExpirydate .$dirty || submitted)" class="help-block red">Please enter Permission Expiryd ate</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>


                <div class="row">
                    <div class="col-md-12" ng-repeat="div in dateRequestList">
                        <label class="control-label col-md-5">Date of {{div}} Request</label>
                        <div class="col-md-7">
                            <div class="form-group date" ng-class="{ 'has-error': ( submitted ||userForm.Date1stRequest.$dirty) && userForm.Date1stRequest.$invalid } ">
                                <div class="input-group input-append date PermissionsInboundDate">
                                    <input type="text" id="Date1stRequest" index="{{$index}}" name="Date1stRequest" class="form-control" onchange="Filldate(this)" ng-model="DateRequest[$index]" placeholder="Date of {{div}} Request">
                                    <span class="input-group-addon add-on"><span class="glyphicon glyphicon-calendar"></span></span>
                                </div>
                                <div class="help-block">
                                    <p ng-show="userForm.Date1stRequest .$error.required && (userForm.Date1stRequest .$dirty || submitted)" class="help-block red">Please enter Date of {{div}} Request</p>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <label class="control-label col-md-5">
                            Acknowledgement line @*<span class="red" ng-show="OtherContractStatus==false && NoResponse==false">*</span>*@
                        </label>
                        <div class="col-md-7">
                            <div class="form-group" @*ng-class="{ 'has-error': (submitted ||userForm.Acknowledgementline.$dirty) && userForm.Acknowledgementline.$invalid } "*@>
                                <textarea class="form-control" ng-model="PermissionsInboundModel.Acknowledgementline" name="Acknowledgementline" 
                                          placeholder="Acknowledgement line" @*ng-required="remarks"*@></textarea>
                                @*<div class="help-block">
                                    <p ng-show="userForm.Acknowledgementline.$error.required && (userForm.Acknowledgementline.$dirty || submitted)">Please enter Acknowledgement line</p>
                                </div>*@
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <label class="control-label col-md-5">Remarks </label>
                        <div class="col-md-7">
                            <div class="form-group">
                                <textarea class="form-control" ng-model="PermissionsInboundModel.InboundRemarks" name="InboundRemarks"
                                          placeholder="Remarks"></textarea>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<script type="text/javascript">
    $(document).ready(function () {
        $(".slideshow").css("display", "none");
        $('.PermissionsInboundDate')
        .datepicker({
            format: "dd/mm/yyyy",
            showButtonPanel: true,
            autoclose: true,
            //  startDate: date,
            //  endDate: TodayDate
        })
        setTimeout(function () { $("select[name*=Currency]").val(6) }, 2000);
    });
    function slideUpDown(obj)
    {
        if($(obj).val()!="")
        {
            $('.slideshow').slideDown('slow');
        }
        else {
            $('.slideshow').slideUp('slow');
        }
    }
    function Filldate(obj) {
        var obj = $(obj);
        angular.element(document.getElementById('angularid')).scope().SetDateValue(obj);
    }
    function FillExpiry(obj) {
        var obj = $(obj);
        angular.element(document.getElementById('angularid')).scope().FillExpiry(obj);
    }
   
</script>