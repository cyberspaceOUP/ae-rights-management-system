@using ACS.Services.Master
@*@model ACS.Core.Domain.Master.AuthorMaster*@
@{
    ViewBag.Title = "AuthorMaster";
    Layout = "~/Views/Shared/_LayoutDesignMaster.cshtml";

}

<style type="text/css">
    .fa {
        color: black !important;
    }

    th {
        text-align: left !important;
    }

    td {
        text-align: left !important;
    }
</style>


@*<script src="~/Content/script/jquery-2.1.0.min.js"></script>*@
<script src="~/js/master/common.master.js"></script>
@*<link href="~/Content/bootstrap-table.min.css" rel="stylesheet" />
    <script src="~/Content/script/jquery.responsiveTabs.js"></script>*@
<script src="~/js/master/Master.Author.js"></script>
<script src="~/js/common/TopSearch.js"></script>
<link rel="stylesheet" href="//cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.3.0/css/datepicker3.min.css" />

@section onInit
{

    angular.element(document.getElementById('angularid')).scope().getAuthorDepartmentList();

    angular.element(document.getElementById('angularid')).scope().GeogList();

}

<div class="container-fluid">
    <div class="row">
        <div class="col-md-12">
            <div class="main-title">
                <h1 id="pageHeaddingAuthor">
                    Author Master
                    @Html.Partial("pagedate")
                </h1>

                <h1 id="pageHeaddingAuthorView" style="display:none;">
                    Author View
                    @Html.Partial("pagedate")
                </h1>
            </div>
        </div>

        @if (Session["PrintPermissions"] != null && TempData["viewId"].ToString() != "0")
        {
            <div class="col-md-12 text-right btnPrint">
                <div class="tools">
                    <input type="button" class="btn green" id="btn_printPage" value="Print">
                </div>
            </div>
        }
        <div class="col-md-12">&nbsp;</div>
        <div class="col-md-12 text-right backToList" style="display:none;">
            <div class="tools">
                <a href="~/Master/Master/AuthorSearch?For=BackToSearch" style="color:black;">Back To List</a>
            </div>
        </div>

        <div class="col-md-12 text-left AuthorCodeValue" style="display:none;">
            <div class="tools">
                <div class="row">
                    <div class="col-md-12">
                        <label class="control-label col-md-5"> <b>Author Code</b> : <span ng-bind="AuthorCode"></span></label>
                    </div>
                </div>
            </div>
        </div>


        <input type="hidden" name="hid_ViewAuthor" id="hid_ViewAuthor" value="@TempData["viewid"]" />
        <!--Author Master entry detail-->
        <form name="userForm" class="form-horizontal" id="FrmInsert" novalidate ng-submit="submitForm()" ng-cloak @*ng-controller="Load"*@>

            <div id="profileContainer" class="col-md-12">



                <ul class="nav nav-tabs noTabBackground responsive">
                    <li class="nav active Author"><a data-toggle="tab" href="#Author" style="color: #000 !important; font-size: 16px;"> Author Details</a></li>

                    <li class="nav Affiliation"><a data-toggle="tab" href="#Affiliation" style="color: #000 !important; font-size: 16px;">Affiliation Details</a></li>



                    <li class="nav Beneficiary" ng-show="BenificaryHide"><a data-toggle="tab" href="#Beneficiary" style="color: #000 !important; font-size: 16px;">Beneficiary Details</a></li>
                    <li class="nav Nominee"><a data-toggle="tab" href="#Nominee" style="color: #000 !important; font-size: 16px;">Nominee Details</a></li>

                </ul>
                <div class="tab-content responsive">


                    <div id="Author" class="tab-pane fade in active tabOpac form" ng-controller="AuthorTab">
                        <div class="panel panel-default" style="border-top: none;">
                            <div class="panel-heading" style="background-color: #ee5941 !important; color: #FFF !important; font-size: 18px; ">Author Details </div>

                            <div class="panel-body">
                                <div class="portlet-body form">
                                    <div class="form-group">
                                        <div class="row">
                                            <div class=" row">
                                                <div class="col-md-12">



                                                    <div class="form-group">

                                                        <label class="checkbox" style="padding-left: 65px;">

                                                            <input type="checkbox" style="float:right;" ng-model="string" id="CheckNominee" ng-checked="true" name="CheckNominee" ng-click="ShowNominee_Detials()" />

                                                            Beneficiary details same as Author details
                                                        </label>

                                                    </div>



                                                </div>
                                            </div>
                                            <div class="col-md-12">
                                                <div class="col-md-6">
                                                    
                                                    <div class=" row">
                                                        <div class="col-md-12">
                                                            <label class="control-label col-md-5">Type <span class="red">*</span></label>
                                                            <div class="col-md-7">
                                                                <div class="form-group">

                                                                    <label class="radio-inline">
                                                                        <input type="radio" name="Type" ng-model="Type" value="Individual" required> Individual

                                                                    </label>

                                                                    <label class="radio-inline">
                                                                        <input type="radio" name="Type" ng-model="Type" value="Institute" required> Institute

                                                                    </label>

                                                                </div>
                                                                <div ng-class="{ 'has-error' : userForm.Type.$invalid && (userForm.Type.$dirty || submitted)}">
                                                                    <div class="help-block">
                                                                        <p ng-show="userForm.Type.$error.required && (userForm.Type.$dirty || submitted)" class="help-block">Please enter Author Type</p>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    
                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <label class="control-label col-md-5">Author SAP Code @*<span class="red">*</span>*@</label>
                                                            <div class="col-md-7">
                                                                <div class="form-group" @*ng-class="{ 'has-error': ( submitted ||userForm.AuthorSAPCode.$dirty) && userForm.AuthorSAPCode.$invalid } "*@>
                                                                    <input type="text" name="AuthorSAPCode" class="form-control" ng-model="AuthorSAPCode" maxlength="6"
                                                                           @*ng-required="!(AuthorSAPCode.length || AuthorSAPCode.length)"*@ ng-pattern="/^[0-9]*$/" placeholder="Author SAP Code">
                                                                    @*<div class="help-block">
                                                                        <p ng-show="userForm.AuthorSAPCode.$error.required && (userForm.AuthorSAPCode.$dirty || submitted)" class="help-block red">Please enter Author SAP Code.</p>
                                                                        <p ng-show="userForm.AuthorSAPCode.$error.pattern" class="help-block red">Please enter valid Author SAP Code.</p>
                                                                    </div>*@
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <label class="control-label col-md-5">First Name <span class="red">*</span></label>
                                                            <div class="col-md-7">
                                                                <div class="form-group" ng-class="{ 'has-error': ( submitted ||userForm.FirstName.$dirty) && userForm.FirstName.$invalid } ">
                                                                    <input type="text" name="FirstName" id="FirstName" ng-required="!(FirstName.length || LastName.length)" 
                                                                           class="form-control" ng-model="FirstName" ng-pattern="/^[a-zA-Z'.\s]/" placeholder="First Name" maxlength="50">
                                                                    <div class="help-block">
                                                                        <p ng-show="userForm.FirstName.$error.required && (userForm.FirstName.$dirty || submitted)" class="help-block red">Please enter Author First Name</p>

                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>

                                                    </div>


                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <label class="control-label col-md-5">Last Name <span class="red">*</span></label>
                                                            <div class="col-md-7">
                                                                <div class="form-group" ng-class="{ 'has-error': ( submitted ||userForm.LastName.$dirty) && userForm.LastName.$invalid } ">
                                                                    <input type="text" name="LastName" class="form-control" ng-model="LastName" ng-required="!(FirstName.length || LastName.length)"
                                                                           ng-pattern="/^[a-zA-Z'.\s]/" placeholder="Last Name" maxlength="50">
                                                                    <div class="help-block">
                                                                        <p ng-show="userForm.LastName.$error.required && (userForm.LastName.$dirty || submitted)" class="help-block red">Please enter Author Last Name</p>

                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <label class="control-label col-md-5"> Address <span class="red">*</span></label>
                                                            <div class="col-md-7">
                                                                <div class="form-group" ng-class="{ 'has-error': ( submitted ||userForm.Address.$dirty) && userForm.Address.$invalid } ">
                                                                    <textarea name="Address" class="form-control" ng-model="Address" required @*ng-pattern="/^[a-zA-Z'.\s]/"*@ 
                                                                              placeholder="Address"></textarea>
                                                                    <div class="help-block">
                                                                        <p ng-show="userForm.Address.$error.required && (userForm.Address.$dirty || submitted)" class="help-block red">Please enter Author Address </p>

                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>



                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <label class="control-label col-md-5">Residency Status  <span class="red">*</span></label>
                                                            <div class="col-md-7">
                                                                <div class="form-group" ng-class="{ 'has-error': ( submitted ||userForm.ResidencyStatus.$dirty) && userForm.ResidencyStatus.$invalid } ">

                                                                    <label class="radio-inline">
                                                                        @*<input type="radio" name="ResidencyStatus" ng-model="ResidencyStatus" ng-click="function_Resident()"  value="Resident" required> Resident*@
                                                                        <input type="radio" name="ResidencyStatus" ng-model="ResidencyStatus" ng-click="function_Resident()" ng-attr-value="Resident" required> Resident

                                                                    </label>

                                                                    <label class="radio-inline">
                                                                        <input type="radio" name="ResidencyStatus" ng-model="ResidencyStatus" value="Non-Resident" ng-change="function_NonResident()" required> Non-Resident

                                                                    </label>
                                                                    <div ng-class="{ 'has-error' : userForm.ResidencyStatus.$invalid && (userForm.ResidencyStatus.$dirty || submitted)}">
                                                                        <div class="help-block">
                                                                            <p ng-show="userForm.ResidencyStatus.$error.required && (userForm.ResidencyStatus.$dirty || submitted)" class="help-block red">Please enter Author Residency Status</p>

                                                                        </div>
                                                                    </div>

                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="row" id="Authorgeo">
                                                        <div class="col-md-12">
                                                            @Html.Partial("~/Views/Shared/_geoGraphical.cshtml")
                                                        </div>
                                                    </div>

                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <label class="control-label col-md-5">Email <span class="red">*</span></label>
                                                            <div class="col-md-7">
                                                                <div class="form-group" ng-class="{ 'has-error': ( submitted ||userForm.Email.$dirty) && userForm.Email.$invalid } ">
                                                                    <input type="text" name="Email" class="form-control" ng-model="Email" required ng-pattern="/^[^\s]+[^\s]+\.[^\s]{2,}$/"
                                                                           placeholder="Email" maxlength="50">
                                                                    <div class="help-block">
                                                                        <p ng-show="userForm.Email.$error.required && (userForm.Email.$dirty || submitted)" class="help-block red">Please enter Email Id</p>
                                                                        <p ng-show="userForm.Email.$error.pattern" class="help-block red">Please enter valid Email Id</p>

                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                </div>

                                                <div class="col-md-6">

                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <label class="control-label col-md-5">Phone<span class="red">*</span> </label>
                                                            <div class="col-md-7">
                                                                <div class="form-group" ng-class="{ 'has-error': ( submitted ||userForm.Phone.$dirty) && userForm.Phone.$invalid } ">
                                                                    <input type="text" name="Phone" class="form-control" ng-model="Phone" maxlength="15" ng-required="!(Phone.length || Mobile.length)" ng-pattern="/^[0-9\_\- ]*$/" placeholder="Phone">
                                                                    <div class="help-block">
                                                                        <p ng-show="userForm.Phone.$error.required && (userForm.Phone.$dirty || submitted)" class="help-block red">Please enter Phone No.</p>
                                                                        <p ng-show="userForm.Phone.$error.pattern" class="help-block red">Please enter valid Phone No.</p>

                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <label class="control-label col-md-5">Mobile <span class="red">*</span></label>
                                                            <div class="col-md-7">
                                                                <div class="form-group" ng-class="{ 'has-error': ( submitted ||userForm.Mobile.$dirty) && userForm.Mobile.$invalid } ">
                                                                    <input type="text" id="Mobile" name="Mobile" class="form-control Mobile" maxlength="15" ng-model="Mobile" ng-required="!(Phone.length || Mobile.length)" ng-pattern="/^[0-9\_\- ]*$/" @*ng-pattern="/^[789]\d{9}$/"*@ placeholder="Mobile">
                                                                    <div class="help-block">
                                                                        <p ng-show="userForm.Mobile.$error.required && (userForm.Mobile.$dirty || submitted)" class="help-block red">Please enter Mobile No.</p>
                                                                        <p ng-show="userForm.Mobile.$error.pattern" class="help-block red">Please enter valid Mobile No.</p>

                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <label class="control-label col-md-5">Fax </label>
                                                            <div class="col-md-7">
                                                                <div class="form-group" ng-class="{ 'has-error': ( submitted ||userForm.Fax.$dirty) && userForm.Fax.$invalid } ">
                                                                    <input type="text" name="Fax" class="form-control" ng-model="Fax" ng-pattern="/^[0-9]+$/" placeholder="Fax" maxlength="50">
                                                                    <div class="help-block">
                                                                        <p ng-show="userForm.Fax.$error.required && (userForm.Fax.$dirty || submitted)" class="help-block red">Please enter Fax No.</p>
                                                                        <p ng-show="userForm.Fax.$error.pattern" class="help-block red">Please enter valid Fax No.</p>

                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>


                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <label class="control-label col-md-5">PAN <span class="red" ng-show="PANNOReqired">*</span></label>
                                                            <div class="col-md-7">
                                                                <div class="form-group" ng-class="{ 'has-error': ( submitted ||userForm.PANNo.$dirty) && userForm.PANNo.$invalid } ">
                                                                    <input type="text" name="PANNo" class="form-control" ng-model="PANNo" maxlength="10" ng-required="PANNOReqired" ng-pattern="/^([a-zA-Z]){5}([0-9]){4}([a-zA-Z]){1}?$/" placeholder="PAN">
                                                                    <div class="help-block">
                                                                        <p ng-show="userForm.PANNo.$error.required && (userForm.PANNo.$dirty || submitted)" class="help-block red">Please enter PAN.</p>
                                                                        <p ng-show="userForm.PANNo.$error.pattern" class="help-block red">Please enter valid PAN.</p>


                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>


                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <label class="control-label col-md-5">Aadhaar Card No </label>
                                                            <div class="col-md-7">
                                                                <div class="form-group" ng-class="{ 'has-error': ( submitted ||userForm.AdharCardNo.$dirty) && userForm.AdharCardNo.$invalid } ">
                                                                    <input type="text" name="AdharCardNo" class="form-control" maxlength="12" ng-pattern="/^[0-9]{12}$/" ng-model="AdharCardNo" placeholder="Aadhaar Card No">
                                                                    <div class="help-block">
                                                                        <p ng-show="userForm.AdharCardNo.$error.required && (userForm.AdharCardNo.$dirty || submitted)" class="help-block red">Please enter Aadhaar Card No</p>
                                                                        <p ng-show="userForm.AdharCardNo.$error.pattern && (userForm.AdharCardNo.$dirty || submitted)" class="help-block red">Please enter valid Aadhaar Card No should be 16 digit</p>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>


                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <label class="control-label col-md-5">Date of Birth <span class="red" ng-show="Type == 'Individual'">*</span></label>
                                                            <div class="col-md-7">
                                                                <div class="form-group" >
                                                                    <div class="input-group input-medium date date-picker DateOfBirth" ng-class="{ 'has-error': ( submitted ||userForm.DateOfBirth.$dirty) && userForm.DateOfBirth.$invalid } ">
                                                                        <input type="text" name="DateOfBirth" id="DateOfBirth" onchange="angular.element(document.getElementById('angularid')).scope().SetDateOfBirth(this);" class="form-control" ng-model="DateOfBirth" ng-required="Type =='Individual'" placeholder="Date of Birth">
                                                                        <span class=" input-group-btn">
                                                                            <button class="btn default date-set" type="button"><i class="fa fa-calendar"></i></button>
                                                                        </span>

                                                                    </div>
                                                                    <div class="help-block">
                                                                        <p ng-show="userForm.DateOfBirth.$error.required && (userForm.DateOfBirth.$dirty || submitted)" style="color: #a94442;" class="help-block red">Please enter Date of Birth</p>

                                                                    </div>

                                                                </div>
                                                            </div>
                                                            <script type="text/javascript">
                                                                $(document).ready(function () {

                                                                    $('.DateOfBirth')
                                                                        .datepicker({
                                                                            format: "dd/mm/yyyy",

                                                                            showButtonPanel: true,
                                                                            autoclose: true,


                                                                        })



                                                                });
                                                            </script>


                                                        </div>
                                                    </div>




                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <label class="control-label col-md-5">Account No <span class="red" ng-show="AccountNoReqired">*</span></label>
                                                            <div class="col-md-7">
                                                                <div class="form-group" ng-class="{ 'has-error': ( submitted ||userForm.AccountNo.$dirty) && userForm.AccountNo.$invalid } ">
                                                                    <input type="text" name="AccountNo" class="form-control" maxlength="18" ng-pattern="/^[0-9]*$/" ng-model="AccountNo" ng-required="AccountNoReqired" @*ng-pattern="/^[1-9]+[0-9]*$/"*@ placeholder="Account No">
                                                                    <div class="help-block">
                                                                        <p ng-show="userForm.AccountNo.$error.required && (userForm.AccountNo.$dirty || submitted)" class="help-block red">Please enter Account No.</p>
                                                                        <p ng-show="userForm.AccountNo.$error.pattern" class="help-block red">Please enter valid Account No.</p>

                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>

                                                    </div>


                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <label class="control-label col-md-5">Bank Name  <span class="red" ng-show="BankNameReq">*</span></label>
                                                            <div class="col-md-7">
                                                                <div class="form-group" ng-class="{ 'has-error': ( submitted ||userForm.BankName.$dirty) && userForm.BankName.$invalid } ">
                                                                    <input type="text" name="BankName" class="form-control" ng-model="BankName" ng-required="BankNameReq" @*ng-pattern="/^[a-zA-Z'.\s]/"*@ placeholder="Bank Name">
                                                                    <div class="help-block">
                                                                        <p ng-show="userForm.BankName.$error.required && (userForm.BankName.$dirty || submitted)" class="help-block red">Please enter Bank Name.</p>

                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>

                                                    </div>


                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <label class="control-label col-md-5">Branch Name <span class="red" ng-show="BranchNameReq">*</span></label>
                                                            <div class="col-md-7">
                                                                <div class="form-group" ng-class="{ 'has-error': ( submitted ||userForm.BranchName.$dirty) && userForm.BranchName.$invalid } ">
                                                                    <input type="text" name="BranchName" class="form-control" ng-model="BranchName" ng-required="BranchNameReq" @*ng-pattern="/^[a-zA-Z'.\s]/"*@ placeholder="Branch Name">
                                                                    <div class="help-block">
                                                                        <p ng-show="userForm.BranchName.$error.required && (userForm.BranchName.$dirty || submitted)" class="help-block red">Please enter Branch Name</p>


                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>

                                                    </div>


                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <label class="control-label col-md-5">Swift / IFSC Code <span class="red" ng-show="IFSECodeReq">*</span></label>
                                                            <div class="col-md-7">
                                                                <div class="form-group" ng-class="{ 'has-error': ( submitted ||userForm.IFSECode.$dirty) && userForm.IFSECode.$invalid } ">
                                                                    <input type="text" name="IFSECode" class="form-control" ng-model="IFSECode" ng-required="IFSECodeReq"
                                                                           placeholder="Swift / IFSC Code" maxlength="20">
                                                                    <div class="help-block">
                                                                        <p ng-show="userForm.IFSECode.$error.required && (userForm.IFSECode.$dirty || submitted)" class="help-block red">Please enter Swift / IFSC Code</p>

                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>

                                                    </div>

                                                    <div class="row">
                                                        <div class="col-md-12">
                                                            <label class="control-label col-md-5">Remark</label>
                                                            <div class="col-md-7">
                                                                <div class="form-group" ng-class="{ 'has-error': ( submitted ||userForm.Remark.$dirty) && userForm.Remark.$invalid } ">
                                                                    <textarea name="Remark" class="form-control" ng-model="Remark" placeholder="Remark"></textarea>
                                                                    <div class="help-block">
                                                                        <p ng-show="userForm.Remark.$error.required && (userForm.Remark.$dirty || submitted)" class="help-block red">Please enter Remark</p>

                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>

                                                    </div>


                                                    @*<div class="row">
                                                            <div class="col-md-12" id="fileuploadcontrol">

                                                            </div>

                                                        </div>*@











                                                    <div class="row" ng-show="false">
                                                        <div class="col-md-12">

                                                            <label class="control-label col-md-5">Author Code</label>
                                                            <div class="col-md-7">
                                                                <div class="form-group" ng-class="{'has-error': ( submitted ||userForm.Code.$dirty) && userForm.Code.$invalid } ">
                                                                    <input type="text" name="Code" class="form-control" ng-model="Code" ng-pattern="/^[a-zA-Z'.\s]/" placeholder="Author Code">
                                                                    <div class="help-block">
                                                                        <p ng-show="userForm.Name.$error.required && (userForm.Name.$dirty || submitted)" class="help-block red">Please enter author code</p>

                                                                    </div>
                                                                </div>
                                                            </div>


                                                        </div>
                                                    </div>





                                                </div>
                                            </div>
                                            <div class="col-md-12">

                                                <div class="col-md-12">

                                                    <label class="control-label col-md-2">Uploaded Document</label>
                                                    <div class="col-md-10">
                                                        <div @*id="fileuploadcontrol"*@ class="fileuploadcontrol" style=" padding-left: 29px;">

                                                        </div>

                                                        <div class="row">
                                                            <div class="col-md-12">
                                                                &nbsp;
                                                            </div>
                                                        </div>

                                                        @*<div id="doclistid" ng-show="true" style=" padding-left: 29px;">*@
                                                        <div id="doclistid" ng-show="documentshow" style=" padding-left: 29px;">
                                                            <div class="panel panel-warning">
                                                                <div class="panel-heading" style="background-color: #ee5941 !important; color: #FFF !important; ">
                                                                    <h3 class="panel-title">Uploaded Attachment</h3>
                                                                </div>

                                                                <div class="tools">

                                                                    <a href="javascript:;" class="collapse"> </a>
                                                                </div>

                                                                <div class="panel-body">

                                                                    <table class="table table-striped table-hover ">
                                                                        <thead>
