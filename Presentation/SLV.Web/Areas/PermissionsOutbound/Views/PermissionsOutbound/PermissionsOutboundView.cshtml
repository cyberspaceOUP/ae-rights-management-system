                                            <label class="control-label col-md-5 col-xs-6 col-sm-6">Request Date </label>
                                            <div class="col-md-7 col-xs-6 col-sm-6">
                                                <div class="form-group">
                                                    <label class="control-label"><span ng-bind="RequestDateView"></span></label>
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-12">
                                            <label class="control-label col-md-5 col-xs-6 col-sm-6">Licensee Publication Title </label>
                                            <div class="col-md-7 col-xs-6 col-sm-6">
