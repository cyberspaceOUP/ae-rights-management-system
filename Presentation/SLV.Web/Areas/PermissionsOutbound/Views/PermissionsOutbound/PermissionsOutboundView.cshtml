                                            <label class="control-label col-md-5 col-xs-6 col-sm-6">Permission Date </label>
                                            <div class="col-md-7 col-xs-6 col-sm-6">
                                                <div class="form-group">
                                                    <label class="control-label"><span ng-bind="PermissionDateView"></span></label>
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                    <div class="row" ng-show="false">
                                        <div class="col-md-12">
                                            <label class="control-label col-md-5 col-xs-6 col-sm-6">Permission period (In months) </label>
                                            <div class="col-md-7 col-xs-6 col-sm-6">
                                                <div class="form-group">
                                                    <label class="control-label"><span ng-bind="PermissionperiodView"></span></label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row" ng-show="false">
                                        <div class="col-md-12">
                                            <label class="control-label col-md-5 col-xs-6 col-sm-6">Expiry Date</label>
                                            <div class="col-md-7 col-xs-6 col-sm-6">
                                                <div class="form-group">
                                                    <label class="control-label"><span ng-bind="ExpiryDateView"></span></label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-12">
                                            <label class="control-label col-md-5 col-xs-6 col-sm-6">Requested Material </label>
                                            <div class="col-md-7 col-xs-6 col-sm-6">
                                                <div class="form-group">
                                                    <label class="control-label"><span ng-bind="RequestMaterialView"></span></label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class=" row">
                                        <div class="col-md-12">
                                            <label class="control-label col-md-5 col-xs-6 col-sm-6">Will the material be translated </label>
                                            <div class="col-md-7 col-xs-6 col-sm-6">
                                                <div class="form-group">
                                                    <label class="control-label"><span ng-bind="WillmaterialtranslatedView"></span></label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class=" row">
                                        <div class="col-md-12">
                                            <label class="control-label col-md-5 col-xs-6 col-sm-6">Will the material be adapted </label>
                                            <div class="col-md-7 col-xs-6 col-sm-6">
                                                <div class="form-group">
                                                    <label class="control-label"><span ng-bind="WillmaterialadeptedView"></span></label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    @*<div class="row">
                                            <div class="col-md-12">
                                                <label class="control-label col-md-5 col-xs-6 col-sm-6">Language</label>

                                                <div class="col-md-7 col-xs-6 col-sm-6">
                                                    <div class="form-group">
                                                        <label class="control-label"><span ng-bind="LanguageView"></span></label>
                                                    </div>
                                                </div>

                                            </div>
                                        </div>*@

                                    <div class="row">
                                        <div class="col-md-12">
                                            <label class="control-label col-md-5 col-xs-6 col-sm-6">Extent</label>

                                            <div class="col-md-7 col-xs-6 col-sm-6">
                                                <div class="form-group">
                                                    <label class="control-label"><span ng-bind="ExtentView"></span></label>
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-12">
                                            <label class="control-label col-md-5 col-xs-6 col-sm-6">Territory </label>

                                            <div class="col-md-7 col-xs-6 col-sm-6">
                                                <div class="form-group">
                                                    <label class="control-label"><span ng-bind="TerritoryView"></span></label>
                                                </div>
                                            </div>

                                        </div>
                                    </div>
                                </div>

                                <div class="col-md-6 col-xs-6 col-sm-6">
                                    
                                    <div class=" row">
                                        <div class="col-md-12">
                                            <label class="control-label col-md-5 col-xs-6 col-sm-6">Invoice Applicable </label>

                                            <div class="col-md-7 col-xs-6 col-sm-6">
                                                <div class="form-group">
                                                    <label class="control-label"><span ng-bind="InvoiceApplicableView"></span></label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row" ng-show="InvoiceApplicableView == 'Yes'">
                                        <div class="col-md-12">
                                            <label class="control-label col-md-5 col-xs-6 col-sm-6">Date of Invoice</label>

                                            <div class="col-md-7 col-xs-6 col-sm-6">
                                                <div class="form-group">
                                                    <label class="control-label"><span ng-bind="DateInvoiceView"></span></label>
                                                </div>
                                            </div>

                                        </div>
                                    </div>

                                    <div class="row" ng-show="InvoiceApplicableView == 'Yes'">
                                        <div class="col-md-12">
                                            <label class="control-label col-md-5 col-xs-6 col-sm-6">Invoice Number </label>
                                            <div class="col-md-7 col-xs-6 col-sm-6">
                                                <div class="form-group">
                                                    <label class="control-label"><span ng-bind="InvoiceNoView"></span></label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row" ng-show="InvoiceApplicableView == 'Yes'">
                                        <div class="col-md-12">
                                            <label class="control-label col-md-5">Invoice Currency</label>
                                            <div class="col-md-7">
                                                <div class="form-group">
                                                    @*<label class="control-label"><span ng-bind="InvoiceCurrencyView"></span></label>*@
                                                    <label class="control-label"><span ng-bind="InvoiceCurrencySymbol"></span></label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row" ng-show="InvoiceApplicableView == 'Yes'">
                                        <div class="col-md-12">
                                            <label class="control-label col-md-5 col-xs-6 col-sm-6">Invoice Value </label>
                                            <div class="col-md-7 col-xs-6 col-sm-6">
                                                <div class="form-group">
                                                    <label class="control-label"><span ng-bind="InvoiceValueView"></span></label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row" ng-show="InvoiceApplicableView == 'Yes'"> @*ReqInvoiceDescription &&*@ 
                                        <div class="col-md-12">
                                            <label for="textArea" class="control-label  col-md-5 col-xs-6 col-sm-6">Invoice Description </label>
                                            <div class="col-md-7 col-xs-6 col-sm-6">
                                                <div class="form-group">
                                                    <label class="control-label"><span ng-bind="InvoiceDescriptionView"></span></label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-12">
                                            <label class="control-label col-md-5 col-xs-6 col-sm-6">Copies to be received  </label>
                                            <div class="col-md-7 col-xs-6 col-sm-6">
                                                <div class="form-group">
                                                    <label class="control-label"><span ng-bind="CopiesreceivedView"></span></label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-12">
                                            <label class="control-label col-md-5 col-xs-6 col-sm-6">Number of copies </label>
                                            <div class="col-md-7 col-xs-6 col-sm-6">
                                                <div class="form-group">
                                                    <label class="control-label"><span ng-bind="NumbercopiesView"></span></label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-12">
                                            <label class="control-label col-md-5 col-xs-6 col-sm-6">Remarks </label>
                                            <div class="col-md-7 col-xs-6 col-sm-6">
                                                <div class="form-group">
                                                    <label class="control-label"><span ng-bind="RemarksView"></span></label>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <!-- Language Details  view-->
            <div class="col-md-12" ng-if="PermissionsOutboundLanguageList.length">
                <div class="portlet box green">
                    <div class="portlet-title">
                        <div class="caption">
                            Language Details
                        </div>
                        <div class="tools">
                            <a href="javascript:;" class="collapse" data-original-title="" title=""> </a>
                        </div>
                    </div>
                    <div class="portlet-body clearfix">
                        <div class="col-md-12">
                            <div class="row">

                                <table class="table table-striped table-bordered table-hover dataTable no-footer SeriesDetail" id="TblOwnerList">
                                    <thead class="flip-content">
                                        <tr>
                                            <th>S.No.</th>
                                            <th>Language</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr ng-repeat="M in PermissionsOutboundLanguageList">
                                            <td><span ng-bind="$index +1"></span></td>
                                            <td>
                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <div class="col-md-12">
                                                            <div class="form-group">
                                                                <label class="control-label"><span ng-bind="M.LanguageName"></span></label>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>

                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <!-- Type of Rights Details  view-->
            <div class="col-md-12">
                <div class="portlet box green">
                    <div class="portlet-title">
                        <div class="caption">
                            Type of Rights Details
                        </div>
                        <div class="tools">
                            <a href="javascript:;" class="collapse" data-original-title="" title=""> </a>
                        </div>
                    </div>
                    <div class="portlet-body clearfix">
                        <div class="col-md-12">
                            <div class="row">

                                <table class="table table-striped table-bordered table-hover dataTable no-footer SeriesDetail" id="TblOwnerList">
                                    <thead class="flip-content">
                                        <tr>
                                            <th>S.No.</th>
                                            <th>Type of Rights</th>
                                            @*<th>Sub-Type Rights </th>*@
                                            <th>Quantity </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr ng-repeat="M in PermissionsOutboundList">
                                            <td><span ng-bind="$index +1"></span></td>
                                            <td>
                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <div class="col-md-12">
                                                            <div class="form-group">
                                                                <label class="control-label"><span ng-bind="M.TypeofRights"></span></label>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </td>
                                            @*<td>
                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <div class="col-md-12">
                                                            <div class="form-group">
                                                                <label class="control-label"><span ng-bind="M.SubTypeRights"></span></label>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </td>*@
                                            <td>
                                                <div class="row">
                                                    <div class="col-md-12">
                                                        <div class="col-md-12">
                                                            <div class="form-group">
                                                                <label class="control-label" ng-if="M.Quantity == '' ||  M.Quantity == 'null'">--</label>

                                                                <label class="control-label" ng-if="M.Quantity != '' &&  M.Quantity != 'null'"><span ng-bind="M.Quantity"></span></label>
                                                               
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>

                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div id="PendingRequest" class="col-md-12" ng-show="ReqPendingRequest">
                <div class="portlet box green">
                    <div class="portlet-title">
                        <div class="caption">
                            Pending Request for Permission Outbound
                        </div>
                        <div class="tools">
                            <a href="javascript:;" class="collapse" data-original-title="" title=""> </a>
                        </div>
                    </div>
                    <div class="portlet-body clearfix">
                        <div class="col-md-12">
                            <div class="row">



                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="col-md-6 col-xs-6 col-sm-6">
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <label class="control-label col-md-5 col-xs-6 col-sm-6"> Contract Status </label>
                                                    <div class="col-md-7 col-xs-6 col-sm-6">
                                                        <div class="form-group">
                                                            <label class="control-label">
                                                                @*<span ng-bind="ContractStatusView"></span>*@
                                                                <span ng-bind="ContractStatusView == 'Pending'? 'Issued' : (ContractStatusView == 'Issued'?'Received':ContractStatusView)"></span>
                                                            </label>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="row">
                                                <div class="col-md-12">
                                                    <label class="control-label col-md-5 col-xs-6 col-sm-6">Payment Received  </label>
                                                    <div class="col-md-7 col-xs-6 col-sm-6">
                                                        <div class="form-group">
                                                            <label class="control-label" ng-show="InvoiceApplicableView == 'Yes'"><span ng-bind="PaymentReceivedView"></span></label>
                                                            <label class="control-label" ng-show="InvoiceApplicableView == 'No'">Not Applicable</label>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
