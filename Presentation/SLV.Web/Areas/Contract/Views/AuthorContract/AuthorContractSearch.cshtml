<div class="container-fluid">
    <div class="row">
        <div class="col-md-12">
            <div class="main-title">
                <h1>
                    <span ng-show="ShowSearchForm">Assignment Contract Search</span>
                    <span ng-show="ShowListForm">
                        @if (ViewBag.Report == "report")
                        {
                            <span>Assignment Contract Report </span>
                        }
                        else
                        {
                            if (ViewBag.ViewMore == "notagreement")
                            { 
                                <span>@T("Dashboard.NoContract")</span>
                            }
                            if (ViewBag.ViewMore == "IssuedDraftAC")
                            { 
                                <span>Assignment Contract Request Status - Assignment Contract List @*- Draft and Issued Contract*@</span>
                            }
                            else if (ViewBag.ViewMore == "NoInbound")
                            {
                                <span>Assignment Contract - @T("Dashboard.InboundNotEntered") </span>
                            }
                            else if (ViewBag.ViewMore == "ACStatus")
                            {
                                <span>@T("Dashboard.AssignmentContractStatus") </span>
                            }
                            else if (ViewBag.ViewMore == "PendingSeries")
                            {
                                <span>&nbsp;</span>
                            }
                            else if (ViewBag.ViewMore == "IssuedDraftSeries")
                            {
                                <span>&nbsp;</span>
                            }
                            else
                            { 
                                <span>Assignment Contract List </span>
                            }
                        }
                    </span>
                    <span ng-show="ShowProductSeriesListForm">Series Contract</span>

                    @Html.Partial("pagedate")
                </h1>
            </div>
        </div>
