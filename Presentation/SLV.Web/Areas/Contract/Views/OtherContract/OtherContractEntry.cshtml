            @*Div for Contract detail entry*@
            <div id="ContractDetailEntryMode" class="col-md-12" ng-show="ContractDetailEntryMode">
                <div class="portlet box green">
                    <div class="portlet-title">
                        <div class="caption">
                            Contract Details
                        </div>
                        <div class="tools">
                            <a href="javascript:;" class="collapse"> </a>
                        </div>
                    </div>
                    <div class="portlet-body form">
                        <div class="form-body">
                            <div class=" row">
                                <div class="col-md-12">
                                    <div class="col-md-6">
                                        <div class="row">
                                            <div class="col-md-12">
                                                <label class="control-label col-md-5">Request Date&nbsp;<span class="red">*</span> </label>
                                                <div class="col-md-7">
                                                    <div class="form-group" ng-class="{ 'has-error': (userForm.RequestDate.$dirty || submitted) && userForm.RequestDate.$invalid }">
                                                        <div class="input-group input-medium date date-picker" data-date-start-date="+0d" id="RequestDateId" @*ng-readonly="true"*@>
                                                            <input type="text" size="16" @*ng-readonly="true"*@ ng-disabled="true" class="form-control" name="RequestDate" id="RequestDate"
                                                                   ng-model="RequestDate">
                                                            <span class=" input-group-btn">
                                                                <button class="btn default date-set" type="button" ng-disabled="true"><i class="fa fa-calendar"></i></button>
                                                            </span>
                                                        </div>
                                                        <div class="help-block">
                                                            <p ng-show="userForm.RequestDate.$error.required && (userForm.RequestDate.$dirty || submitted)">Please select request date</p>

                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>


                                        <div class="row">
                                            <div class="col-md-12">
                                                <label class="control-label col-md-5">Project Title</label>
                                                <div class="col-md-7">
                                                    <div class="form-group" ng-class="{ 'has-error': ( submitted || userForm.ProjectTitle.$dirty) && userForm.ProjectTitle.$invalid } ">
                                                        <input type="text" maxlength="100" name="ProjectTitle" class="form-control" ng-model="ProjectTitle" placeholder="Project Title">
                                                        <div class="help-block">
                                                            <p ng-show="userForm.ProjectTitle.$error.pattern" class="help-block red">Please enter valid mobile number</p>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>



                                        <div class="row">
                                            <div class="col-md-12">
                                                <label class="control-label col-md-5">Project ISBN</label>
                                                <div class="col-md-7">
                                                    <div class="form-group" ng-class="{ 'has-error': ( submitted || userForm.ProjectIsbn.$dirty) && userForm.ProjectIsbn.$invalid } ">
                                                        <input type="text" name="ProjectIsbn" class="form-control" ng-model="ProjectIsbn" maxlength="13" ng-pattern="/^[0-9]{13}$/" placeholder="Project ISBN">
                                                        <div class="help-block">
                                                            <p ng-show="userForm.ProjectIsbn.$error.pattern" class="help-block red">Please enter valid ISBN number</p>

                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>


                                        <div class="row">
                                            <div class="col-md-12">
                                                <label class="control-label col-md-5">Contract Type&nbsp;<span class="red">*</span></label>
                                                <div class="col-md-7">
                                                    <div class="form-group" ng-class="{ 'has-error': ( submitted ||userForm.ContractType.$dirty) && userForm.ContractType.$invalid } ">
                                                        <select id="ddlContractType" class="form-control" name="ContractType" ng-model="ContractType" ng-required="ContractTypeReq" ng-change="GetForImageBank(ContractType)">
                                                            <option value="">Please Select</option>
                                                            <option ng-repeat="ContractType in ContractTypeList" value="{{ContractType.Id}}" ng-selected="{{ContractType.Id == ContractType}}">{{ContractType.ContractName}}</option>


                                                        </select>

                                                        <input type="hidden" id="ContactType" />
                                                        <div class="help-block">
                                                            <p ng-show="userForm.ContractType.$error.required && (userForm.ContractType.$dirty || submitted)">Please select contract type</p>
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>


                                        <div class="row">
                                            <div class="col-md-12">
                                                <label class="control-label col-md-5">Division&nbsp;<span class="red">*</span></label>
                                                <div class="col-md-7">
                                                    <div class="form-group" ng-class="{ 'has-error': ( submitted ||userForm.Division.$dirty) && userForm.Division.$invalid } ">
