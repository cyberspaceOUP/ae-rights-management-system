<div class="portlet-body clearfix">
    <div class="portlet-body flip-scroll">
        <table class="table table-striped table-bordered table-hover dataTable no-footer RoyaltySlab" id="TblOwnerList">
            <thead class="flip-content">
                <tr>
                    <th>S No.</th>
                    <th>Product Sub-Type</th>
                    <th>Copies From</th>
                    <th>Copies To</th>
                    <th>Percentage
                    <th>
                </tr>
            </thead>
            <tbody>
                <tr ng-repeat="i in RoyaltyslabList_Detail | filter:{AuthorId:div.Id}">
                    <td><span ng-bind="$index +1"></span></td>
                    <td>
                        <div class="row">
                            <div class="col-md-12">
                                <div class="col-md-12">
                                    <div class="form-group">
                                        <label><span ng-bind="i.SubProductType"></span></label>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </td>
                    <td>
                        <div class="row">
                            <div class="col-md-12">
                                <div class="col-md-12">
                                    <label><span ng-bind="i.CopiesFrom"></span></label>
                                </div>
                            </div>
                        </div>
                    </td>
                    <td>
                        <div class="row">
                            <div class="col-md-12">
                                <div class="col-md-12">
                                    <label><span ng-bind="i.CopiesTo==0?'---':i.CopiesTo"></span></label>
                                </div>
                            </div>
                        </div>
                    </td>
                    <td colspan="2">
                        <div class="row">
                            <div class="col-md-12">
                                <div class="col-md-12">
                                    <label><span ng-bind="i.Percentage"></span></label>
                                </div>
                            </div>
                        </div>
                    </td>
                </tr>
            </tbody>
        </table>
    </div>
</div>
