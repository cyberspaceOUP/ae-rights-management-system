
<div class="portlet box green" ng-show="AgreementStatus.length">
    <div class="portlet-title">
        <div class="caption">
          @*Assignment Contract Agreement*@ Agreement Details
        </div>
        <div class="tools">
            <a href="javascript:void(0);" class="collapse" data-original-title="" title=""> </a>
        </div>
    </div>
    <div class="portlet-body clearfix">
        <div class="col-md-6 col-xs-6 col-sm-6">
            <div class="col-md-12">
                <label class="control-label col-md-5 col-xs-6 col-sm-6">Contract status</label>
                <div class="col-md-7 col-xs-6 col-sm-6">
                    <label class="control-label">
                        @*<span ng-bind="AgreementStatus == 'Pending'? 'Issued' : (AgreementStatus == 'Issued'?'Received':AgreementStatus)"></span>*@
                        <span ng-bind="AgreementStatus | Filter_AgreementStatus"></span>
                    </label>
                </div>
            </div>
            <div ng-show="AgreementStatus!='Cancelled' && AgreementStatus!='Draft' && AgreementStatus!=''">
                <div class="col-md-12" ng-show="AgreementStatus=='Issued'">
                    <label class="control-label col-md-5 col-xs-6 col-sm-6">Date of Agreement</label>
                    <div class="col-md-7 col-xs-6 col-sm-6">
                        <label class="control-label"><span ng-bind="AgreementDate"></span></label>
                    </div>
                </div>
                <div class="col-md-12" ng-show="AgreementStatus=='Issued' && TemsOfCopyRight==1">
                    <label class="control-label col-md-5 col-xs-6 col-sm-6">Effective Date</label>
                    <div class="col-md-7 col-xs-6 col-sm-6" ng-show="EffectiveDate !=null">
                        <label class="control-label"><span ng-bind="EffectiveDate"></span></label>
                    </div>
                    <div class="col-md-7 col-xs-6 col-sm-6" ng-show="EffectiveDate ==null">
                        <label class="control-label">---</label>
                    </div>
                </div>
                @*<div class="col-md-12" ng-show="AgreementStatus=='Issued' && TemsOfCopyRight==1">
                    <label class="control-label col-md-5 col-xs-6 col-sm-6">Period of Agreement (In Months) </label>
                    <div class="col-md-7 col-xs-6 col-sm-6">
                        <label class="control-label"> <span ng-bind="PeriodinMonth"></span></label>
                    </div>
                </div>*@
                <div class="col-md-12" ng-show="AgreementStatus=='Issued' && TemsOfCopyRight==1">
                    <label class="control-label col-md-5 col-xs-6 col-sm-6">Expiry Date</label>
                    <div class="col-md-7 col-xs-6 col-sm-6" ng-show="ExpiryDate !=null">
                        <label class="control-label"> <span ng-bind="ExpiryDate"></span></label>
                    </div>

                    <div class="col-md-7 col-xs-6 col-sm-6" ng-show="ExpiryDate == null">
                        <label class="control-label"> ---</label>
                    </div>
                </div>


                <div class="col-md-12" ng-show="AgreementStatus=='Issued'">
                    <label class="control-label col-md-5 col-xs-6 col-sm-6">Signed Contract received date</label>
                    <div class="col-md-7 col-xs-6 col-sm-6">
                        <label class="control-label"><span ng-bind="contractRecieved==null?'---':contractRecieved"></span></label>
                    </div>
                </div>


                <div class="col-md-12" ng-show="AgreementStatus=='Issued'">
                    <label class="control-label col-md-5 col-xs-6 col-sm-6">Signed Contract sent date</label>
                    <div class="col-md-7 col-xs-6 col-sm-6">
                        <label class="control-label"><span ng-bind="SignedcontracDate==null?'---':SignedcontracDate"></span></label>
                    </div>
                </div>

                @*<div class="col-md-12 col-xs-12 col-sm-12" ng-show="AgreementStatus=='Issued'">
                    <label class="control-label col-md-5 col-xs-6 col-sm-6">Author copies sent date</label>
                    <div class="col-md-7 col-xs-6 col-sm-6">
                        <label class="control-label"><span ng-bind="AuthorCopiesSend"></span></label>
                    </div>
                </div>


                <div class="col-md-12 col-xs-12 col-sm-12" ng-show="AgreementStatus=='Issued'">
                    <label class="control-label col-md-5 col-xs-6 col-sm-6">Contributor copies sent date</label>
                    <div class="col-md-7 col-xs-6 col-sm-6">
                        <label class="control-label"><span ng-bind="CotributorCopiessend"></span></label>
                    </div>
                </div>*@

                <div class="col-md-12 col-xs-12 col-sm-12">
                    <label class="control-label col-md-5 col-xs-6 col-sm-6"><span ng-bind="AgreementStatus"></span> Remarks</label>
                    <div class="col-md-7 col-xs-6 col-sm-6">
                        <label class="control-label"> <span ng-bind="ContractRemarks"></span></label>
                    </div>
                </div>
            </div>
            <div class="panel1" ng-show="AgreementStatus=='Cancelled' || AgreementStatus == 'Draft'">
                <div class="col-md-12 col-xs-12 col-sm-12">
                    <label class="control-label col-md-5 col-xs-6 col-sm-6"><span ng-bind="AgreementStatus =='Draft' ? 'Draft Date' : 'Cancellation date'"></span></label>
                    <div class="col-md-7 col-xs-6 col-sm-6">
                        <label class="control-label"><span ng-bind="CancelDate"></span></label>
                    </div>
                </div>
                <div class="col-md-12 col-xs-12 col-sm-12">
                    <label class="control-label col-md-5 col-xs-6 col-sm-6"><span ng-bind="AgreementStatus =='Draft' ? 'Draft Remarks' : 'Cancellation Reason'"></span></label>
                    <div class="col-md-7 col-xs-6 col-sm-6">
                        <label class="control-label"> <span ng-bind="CancellationRemarks"></span></label>
                    </div>
                </div>
            </div>

            <div class="panel-body" ng-if="AgreementDoc.length>0">

                <table class="table table-striped table-hover ">
                    <thead>
                        <tr>
                            <th>S.No</th>
                            <th>Document Name</th>

                        </tr>
                    </thead>
                    <tbody>
                        <tr ng-repeat="url in AgreementDoc">
                            <td><span ng-bind="$index + 1"></span> <input type="hidden" name="DocId_{{$index}}" value="{{url.Id}}" /></td>
                            <td>
                                <a href="~/uploads/{{url.FileName}}" download="{{url.FileName}}" target="_blank" style="color: black;" class="hide_for_print"><span ng-bind="url.FileNameEntered"></span></a>
                                <a style="color: black; display: none; " class="show_for_print">{{url.FileNameEntered}}</a>
                            </td>
                        </tr>
                    </tbody>
                </table>

            </div>
        </div>
        <div class="col-md-6 col-xs-6 col-sm-6">
            <div class="row">
                <div class="col-md-12">
                    <label class="control-label col-md-5 col-xs-6 col-sm-6">Contributor Agreement</label>
                    <div class="col-md-7 col-xs-6 col-sm-6">
                        <label class="control-label"> <span ng-bind="ContributorAgreement"></span></label>

                    </div>
                </div>
            </div>
            <div class="panel-body" ng-if="ContributorDoc.length>0">
                <table class="table table-striped table-hover ">
                    <thead>
                        <tr>
                            <th>S.No</th>
                            <th>Document Name</th>

                        </tr>
                    </thead>
                    <tbody>
                        <tr ng-repeat="url in ContributorDoc">
                            <td><span ng-bind="$index + 1"></span> <input type="hidden" name="DocId_{{$index}}" value="{{url.Id}}" /></td>
                            <td>
                                <a href="~/uploads/{{url.FileName}}" download="{{url.FileName}}" target="_blank" style="color: black;" class="hide_for_print"><span ng-bind="url.FileNameEntered"></span></a>
                                <a href="#" style="color: black; display: none;" class="show_for_print">{{url.FileNameEntered}}</a>
                            </td>
                        </tr>
                    </tbody>
                </table>

            </div>
        </div>
    </div>


</div>
