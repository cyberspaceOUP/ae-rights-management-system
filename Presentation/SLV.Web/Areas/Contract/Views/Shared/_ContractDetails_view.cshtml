
<div class="portlet-body clearfix">
    <div class="portlet-body flip-scroll">
        <table class="table table-striped table-bordered table-hover dataTable no-footer" id="TblOwnerList">
            <thead class="flip-content">
                <tr>
                    <th>Payment Period</th>
                    <th>Author Copies</th>
                    <th>Seed Money</th>
                    <th>One-Time Payment</th>
                    <th>Advance Royalty</th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <td>
                        <div class="row">
                            <div class="col-md-12">
                                <div class="col-md-12">
                                   <label class="control-label"><span ng-bind='div.PaymentPeriod!=""?div.PaymentPeriod:"---"'></span></label>
                                </div>
                            </div>
                        </div>
                    </td>
                    <td>
                        <div class="row">
                            <div class="col-md-12">
                                <div class="col-md-12">
                                   <label class="control-label"><span ng-bind='div.AuthorCopies!=null?div.AuthorCopies:"---"'></span></label>
                                </div>
                            </div>
                        </div>
                    </td>
                    <td>
                        <div class="row">
                            <div class="col-md-12">
                               <label class="control-label"><span ng-bind='div.SeedMoney!=null?div.SeedMoney:"---"'></span></label>
                            </div>
                        </div>
                    </td>
                    <td>
                        <div class="row">
                            <div class="col-md-12">
                               <label class="control-label"><span ng-bind='div.OneTimePayment!=null?div.OneTimePayment:"---"'></span></label>
                            </div>
                        </div>
                    </td>
                    <td>
                        <div class="row">
                            <div class="col-md-12">
                              <label class="control-label"><span ng-bind='div.AdvanceRoyalty!=null?div.AdvanceRoyalty:"---"'></span></label>
                            </div>
                        </div>
                    </td>
                </tr>
            </tbody>
        </table>
    </div>
</div>