@using ACS.Services.Master
@{
    ViewBag.Title = "LicenseeList";
    Layout = "~/Views/Shared/_LayoutDesignMaster.cshtml";
}

<script src="~/js/list/List.Licensee.js"></script>
<script src="~/js/master/common.master.js"></script>
<script src="~/js/common/TopSearch.js"></script>
@section onInit
{
    angular.element(document.getElementById('angularid')).scope().getAllLicensee();
}

<div class="container-fluid">
    <div class="row">
        <div class="col-md-12">
            <div class="main-title">
                <h1>
                    Licensee List
                    @Html.Partial("pagedate")
                </h1>
            </div>
        </div>
        <div class="col-md-12">
            <div class="portlet box green">
                <div class="portlet-title">
                    <div class="caption">
                        Licensee List
                    </div>
                    <div class="tools">
                        <a href="javascript:;" class="collapse"> </a>
                    </div>
                </div>
                <div class="portlet-body flip-scroll">
                    <div class="row">
                        <div class="col-md-12">
                            &nbsp;
                        </div>
                    </div>

                    <table datatable="ng" class="table table-bordered table-striped table-condensed flip-content">
                        <thead class="flip-content">
                            <tr>
                                <th>Organisation Name &nbsp;<span class="glyphicon glyphicon-sort"></span></th>
                                <th>Contact Person &nbsp;<span class="glyphicon glyphicon-sort"></span></th>
                                <th>Address &nbsp;<span class="glyphicon glyphicon-sort"></span></th>
                                <th>Country &nbsp;<span class="glyphicon glyphicon-sort"></span></th>
                                <th>State &nbsp;<span class="glyphicon glyphicon-sort"></span></th>
                                <th>City &nbsp;<span class="glyphicon glyphicon-sort"></span></th>
                                <th>Mobile &nbsp;<span class="glyphicon glyphicon-sort"></span></th>
                                <th>Email &nbsp;<span class="glyphicon glyphicon-sort"></span></th>
                                <th>URL &nbsp;<span class="glyphicon glyphicon-sort"></span></th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="Licensee in LicenseeList">

                                <td><span ng-bind="Licensee.OrganizationName"></span></td>
                                <td><span ng-bind="Licensee.ContactPerson"></span></td>
                                <td><span ng-bind="Licensee.Address"></span></td>
                                <td><span ng-bind="Licensee.Country"></span></td>
                                <td><span ng-bind="Licensee.State"></span></td>
                                <td><span ng-bind="Licensee.City"></span></td>
                                <td><span ng-bind="Licensee.Mobile"></span></td>
                                <td><span ng-bind="Licensee.Email"></span></td>
                                <td><span ng-bind="Licensee.URL"></span></td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
        </div>


        <div class="clearfix"></div>
        <div class="col-md-12" id="div_btnExcel">
            <div class="form-group text-center">
                <form name="userForm" class=" form-horizontal" ng-show="LicenseeList.length != 0" ng-submit="LicenseeListExcel()">
                    <button type="submit" class="btn green" id="btnSubmit">Generate Excel</button>
                </form>
            </div>
        </div>

    </div>
</div>