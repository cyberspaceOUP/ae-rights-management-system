@using ACS.Services.Master
@{
    ViewBag.Title = "SeriesList";
    Layout = "~/Views/Shared/_LayoutDesignMaster.cshtml";
}

<script src="~/js/list/List.Series.js"></script>
<script src="~/js/master/common.master.js"></script>
<script src="~/js/common/TopSearch.js"></script>

@section onInit
{
    angular.element(document.getElementById('angularid')).scope().GetSeriesList();
}

    <div class="container-fluid">
        <div class="row">
            <div class="col-md-12">
                <div class="main-title">
                    <h1>
                        Series List
                        @Html.Partial("pagedate")
                    </h1>
                </div>
            </div>
            <div class="col-md-12">
                <div class="portlet box green">
                    <div class="portlet-title">
                        <div class="caption">
                            Series List
                        </div>
                        <div class="tools">
                            <a href="javascript:;" class="collapse"> </a>
                        </div>
                    </div>
                    <div class="portlet-body flip-scroll">
                        <div class="row">
                            <div class="col-md-12">
                                &nbsp;
    </div>
</div>

<table datatable="ng" class="table table-bordered table-striped table-condensed flip-content">
    <thead class="flip-content">
        <tr>
            <th>Division Name &nbsp;<span class="glyphicon glyphicon-sort"></span></th>
            <th>Sub-Division Name &nbsp;<span class="glyphicon glyphicon-sort"></span></th>
            <th>Series Name &nbsp;<span class="glyphicon glyphicon-sort"></span></th>
        </tr>
    </thead>
    <tbody>
        <tr ng-repeat="series in SeriesData">

            <td><span ng-bind="series.DivisionName"></span></td>
            <td><span ng-bind="series.SubdivisionName"></span></td>
            <td><span ng-bind="series.SeriesName"></span></td>
        </tr>
    </tbody>
</table>
</div>
</div>
</div>


<div class="clearfix"></div>
<div class="col-md-12" id="div_btnExcel">
    <div class="form-group text-center">
        <form name="userForm" class=" form-horizontal" ng-submit="SeriesListExcel()">
            <button type="submit" class=" btn green" id="btnSubmit">Generate Excel</button>
        </form>
    </div>
</div>

</div>
</div>