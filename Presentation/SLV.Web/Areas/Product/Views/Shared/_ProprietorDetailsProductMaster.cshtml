            <div class="col-md-12">
                <label class="control-label col-md-5">Author Name </label>
                <div class="col-md-7">
                    <div class="form-group" ng-class="{ 'has-error': ( submitted ||userForm.ProprietorAuthorName.$dirty) && userForm.ProprietorAuthorName.$invalid } ">


                        <input type="text" name="ProprietorAuthorName" class="form-control"
                               ng-init="ProprDetailCntrl.ProprietorAuthorName=''"
                               ng-model="ProprDetailCntrl.ProprietorAuthorName" ng-pattern="/^[a-zA-Z'.\s]/" placeholder="Author Name">
                        <div class="help-block">
                            <p ng-show="userForm.ProprietorAuthorName.$error.required && (userForm.ProprietorAuthorName.$dirty || submitted)" class="help-block red">Please enter valid Author Name</p>
