<div class="portlet box green">
    <div class="portlet-title">
        <div class="caption">
            Product License Details
        </div>
        <div class="tools">
            <a href="javascript:;" class="collapse"> </a>
        </div>
    </div>
    <div class="portlet-body">
        <div class="table-responsive">
            <table class="table table-bordered table-striped">
                <tr>
                    <th> Product License Code </th>
                    <th> Company </th>
                    <th> Contact Person </th>
                    <th> Request Date</th>
                    <th> Contract Date </th>
                    <th> Expiry Date </th>
                </tr>
                <tr ng-repeat="License in ProductLicenseDetails">
                    <td>   
                        <a href="~/Product/ProductLicense/view?Id={{License.Id}}" target="_blank" style="color: #ee5941;" class="hide_for_print">
                            <span ng-bind="License.ProductLicensecode"></span>
                        </a>
                            <input type="hidden" id="hid_currencyid" value="{{License.ProjectedCurrencyId}}" />
                            <input type="hidden" id="hid_price" value="{{License.ProjectedPrice}}" />
                    </td>
                    <td><span ng-bind="License.Company"></span></td>
                    <td><span ng-bind="License.ContactPerson"></span></td>
                    <td><span ng-bind="License.RequestDate"></span></td>
                    <td ng-show="License.ContractDate !=null && License.ContractDate !='' ">
                        <span ng-bind="License.ContractDate"></span>
                    </td>

                    <td ng-show="License.ContractDate ==null || License.ContractDate ==''">
                        --
                    </td>

                    <td ng-show="License.ExpiryDate !=null && License.ExpiryDate !=''">
                        <span ng-bind="License.ExpiryDate"></span>


                    </td>
                    <td ng-show="License.ExpiryDate ==null || License.ExpiryDate ==''">
                        --
                    </td>

                </tr>
        </table>
    </div>
</div>
</div>


