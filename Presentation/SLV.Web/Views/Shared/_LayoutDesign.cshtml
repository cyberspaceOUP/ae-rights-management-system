<script type="text/javascript">
     
    renderCurrentApplicationObject();
    $(document).ready(function () {
        renderUserNamePartialView();
        renderSocietyPartialView();
        renderSwitchSocietyPartialView();
        
    });


    function renderUserNamePartialView() {
        $.ajax({
            cache: false,
            type: "GET",
            contentType: 'application/html; charset=utf-8',
            url: "/Home/UserName",
            data: {},
            datatype: 'html',
            success: function (result) {
                $("#divUser").html(result);
            },
            error: function (xhr, ajaxOptions, thrownError) {
            }
        });
    }
    function renderSocietyPartialView() {
        $.ajax({
            cache: false,
            type: "GET",
            contentType: 'application/html; charset=utf-8',
            url: "/Home/SocietyName",
            data: {},
            datatype: 'html',
            success: function (result) {
                $("#divSocietyName").html(result);
            },
            error: function (xhr, ajaxOptions, thrownError) {
            }
        });
    }

    function renderSwitchSocietyPartialView() {
        $.ajax({
            cache: false,
            type: "GET",
            contentType: 'application/html; charset=utf-8',
            url: "/Home/SwitchSociety",
            data: {},
            datatype: 'html',
            success: function (result) {
                $("#ulSwitchSociety").html(result);
                
                $("#switchSocietyCount").html($("#ulSwitchSociety").find(".scount").length);
            },
            error: function (xhr, ajaxOptions, thrownError) {
            }
        });
    }

    function renderCurrentApplicationObject() {
       
        $.ajax({
            cache: false,
            type: "POST",
            contentType: 'application/json; charset=utf-8',
            url: "/Home/CurrentApplicationObject",
            data: {},
            datatype: 'json',
            success: function (result) {
                $("#hdnContactId").val(result.CurrentContact);
         
                $("#hdnSocietyId").val(result.CurrentSociety);
                $("#hdnFlatId").val(result.CurrentFlat);
            },
            error: function (xhr, ajaxOptions, thrownError) {
            }
        });
    }
    window.onpaint = renderCurrentApplicationObject();

</script>
