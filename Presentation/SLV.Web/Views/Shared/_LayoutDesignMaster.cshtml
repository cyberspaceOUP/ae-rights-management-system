    <script type="text/javascript" language="javascript">
        var app = angular
          .module('app', ['ngMessages', 'datatables', 'oitozero.ngSweetAlert', 'blockUI', 'ngSanitize', 'MassAutoComplete']);//
        app.controller('MainCtrl', MainCtrl);

        function MainCtrl($scope, AJService, $window, SweetAlert, blockUI) {
            //app.expandControllerNotification($scope, AJService, $window, SweetAlert, blockUI);         
        }


        app.directive('validFile', function () {
            return {
                require: 'ngModel',
                link: function (scope, el, attrs, ngModel) {
                    ngModel.$render = function () {
                        ngModel.$setViewValue(el.val());
                    };

                    el.bind('change', function () {
                        scope.$apply(function () {
                            ngModel.$render();
                        });
                    });
                }
            };
        });

        app.directive('repeatDone', function () {
            return function (scope, element, attrs) {
                if (scope.$last) { // all are rendered
                    scope.$eval(attrs.repeatDone);
                }
            }
        });


        app.directive('focus',
            function ($timeout) {
                return {
                    scope: {
                        trigger: '@@focus'
                    },
                    link: function (scope, element) {
                        scope.$watch('trigger', function (value) {
                            if (value === "true") {
                                $timeout(function () {
                                    element[0].focus();
                                });
                            }
                        });
                    }
                };
            }
        );

        var GlobalredirectPath;
      
   // var GlobalredirectPath = "http://localhost/RMS/";
        //  var GlobalredirectPath = "http://localhost/FCMWeb/";
         
        //Popup box for session expires
        setTimeout(function () {
            $(function () {
                $("#dialog").dialog();
                $('.ui-dialog-titlebar-close').hide();
                $('.ui-dialog-titlebar').css('background-color', '#ee5941');
            });
        }, 2700000);

        //Start For only Safari browser
        if (navigator.userAgent.search("Safari") >= 0 && navigator.userAgent.search("Chrome") < 0) {
            setTimeout(function () {
                //alert('safari');

                //for Body
                $('body').css('font-family', 'Arial');
                $('body').css('color', '#303030');

                //for DropDown
                $('select').css('line-height', '29px');
                $('select').css('text-indent', '10px');

                //for Checkbox
                $('.radio-inline').find('.checker').parent().css('display', '-webkit-box');
                $('.radio-inline').find('.checker').parent().parent().css('display', '-webkit-box');

                               
            }, 300);
        }
        //End For only Safari browser
        
        /// window.onload = window.history.forward(0); 
         
    </script>
        
    
